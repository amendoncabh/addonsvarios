	<subDataset name="branch_information">
		<parameter name="start_branch" class="java.lang.String"/>
		<parameter name="end_branch" class="java.lang.String"/>
		<queryString>
			<![CDATA[select *
from ( select branch_name start_branch_name,
	      branch_id start_branch_id
       from  pos_branch
       where sequence = $P{start_branch}::integer
       ) as start_branch
cross join (select branch_name as end_branch_name,
		   branch_id as end_branch_id
	    from  pos_branch
	    where sequence = $P{end_branch}::integer
	   ) as end_branch]]>
		</queryString>
		<field name="start_branch_name" class="java.lang.String"/>
		<field name="start_branch_id" class="java.lang.String"/>
		<field name="end_branch_name" class="java.lang.String"/>
		<field name="end_branch_id" class="java.lang.String"/>
	</subDataset>
	<parameter name="start_date" class="java.lang.String"/>
	<parameter name="end_date" class="java.lang.String"/>
	<parameter name="start_branch" class="java.lang.String"/>
	<parameter name="end_branch" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
company_name,
branch_id,
street,
street2,
city,
zip,
phone,
branch_name,
time_order,
(now()+interval '7 hour')::date as print_date,
(now()+interval '7 hour')::time as print_time,
pb_id,
cate_name,
id_cate,
sequence_cate,
default_code,
product,
price_unit as price_unit,
sum_qty as sum_qty,
sum_total as sum_total,
price_subtotal_incl as price_subtotal_incl,
discount_amount as discount_amount,
before_vat as before_vat,
before_vat_discount as before_vat_discount,
date_part( 'isodow',Date(time_order)),
(
	select day_w
	from
	(SELECT 'วันจันทร์ที่' AS "day_w"  ,1 as day_of_week
		UNION
		SELECT 'วันอังคารที่' AS "day_w" ,2
		UNION
		SELECT 'วันพุธที่' AS "day_w"  ,3
		UNION
		SELECT 'วันพฤหัสบดีที่' AS "day_w"  ,4
		UNION
		SELECT 'วันศุกร์ที่' AS "day_w"  ,5
		UNION
		SELECT 'วันเสาร์ที่' AS "day_w"  ,6
		UNION
		SELECT 'วันอาทิตย์ที่' AS "day_w"  ,7) as p
	where  p.day_of_week=date_part( 'isodow', DATE(time_order))
	) as day_of_week
from(
	select
	pb.branch_name,
	pb.branch_id,
	pb.id as pb_id,
	DATE(po.date_order + interval '7 hours') as time_order,
	rpc.name as company_name,
	rpc.street,
	rpc.street2,
	rpc.city,
	rpc.zip,
	rpc.phone,
	pcg.id as id_cate,
	pcg.sequence as sequence_cate,
	pcg.name as cate_name,
	pt.name as product,
	pp.default_code as default_code,
	pol.price_unit as price_unit,
	sum(pol.qty) as sum_qty,
	atx.amount,
	sum(pol.price_unit * pol.qty) as sum_total,
	sum(pol.price_subtotal_incl) as price_subtotal_incl,
	sum(coalesce(case when po.is_void_order is true
				   then coalesce(abs(pol.prorate_amount),0) * -1
				   else coalesce(abs(pol.prorate_amount),0)
	end, 0) + coalesce(free_product.discount_free_prod,0)) as discount_amount,

	      sum(
		  case when atx.amount is not null
	               then ((pol.price_unit * pol.qty) * 100) / (100 + atx.amount)
	               else pol.price_unit * pol.qty
	          end
	         ) as before_vat,

	      sum(
		  case when atx.amount is not null
	               then ((coalesce(case when po.is_void_order is true
				   then pol.prorate_amount * -1
				   else pol.prorate_amount
		               end, 0) + coalesce(free_product.discount_free_prod,0)) * 100) / (100 + atx.amount)
	               else coalesce(case when po.is_void_order is true
				   then pol.prorate_amount * -1
				   else pol.prorate_amount
		               end, 0) + coalesce(free_product.discount_free_prod,0)
	          end
	         ) as before_vat_discount
	from (select *
	      from pos_order
	     where  return_order_id is null  and  is_return_order is not  True 
	      		and  state not in ('draft', 'cancel')
	            and (date_order + interval '7 hours')::date between $P{start_date}::date and $P{end_date}::date     
	     ) po
 	inner JOIN pos_session ps ON po.session_id = ps.id
	inner JOIN pos_config pf ON ps.config_id = pf.id
	inner JOIN (select *
		   from pos_branch
		   where sequence BETWEEN $P{start_branch}::integer and $P{end_branch}::integer
		  ) pb ON pf.branch_id = pb.id
	inner JOIN res_company pc ON pb.pos_company_id = pc.id
	inner JOIN res_partner rpc ON pc.partner_id = rpc.id
	inner join (
		 select id,
		 price_subtotal_incl,
		 discount_amount,
		 order_id,
		     product_id,
		     price_unit,
		     sum(qty) qty,
		     max(prorate_amount) as prorate_amount,
		     promotion
	      from pos_order_line
	      where promotion_name is null
	      group by id,
		       price_subtotal_incl,
		       discount_amount,
		       order_id,
		       product_id,
		       price_unit,
		       promotion
	) pol on po.id = pol.order_id
	left join res_users ru on ru.id=po.company_id
	inner JOIN product_product pp ON pol.product_id = pp.id
	inner JOIN product_template pt ON pp.product_tmpl_id = pt.id
	
	left JOIN product_category pcg ON pt.categ_id = pcg.id
	left join account_tax_pos_order_line_rel atp on atp.pos_order_line_id = pol.id
	left join account_tax atx on atp.account_tax_id=atx.id
	left join (select abs(sum(price_subtotal_incl)) as discount_free_prod,
			  free_product_id,
			  order_id
		   from pos_order_line
		   where free_product_id is not null
		   group by order_id,
			    free_product_id,
			    DATE(create_date + interval '7 hours')
	) as free_product on (free_product.order_id = po.id
			and free_product.free_product_id = pol.product_id) and pol.promotion is true
	group by
	DATE(po.date_order + interval '7 hours') ,
	rpc.id,
	pb.branch_name,
	pt.name,
	pb.branch_id,
	pb.id,
	pcg.id,
	pcg.name,
	pcg.sequence,
	pp.default_code,
	pol.price_unit,
	pol.discount_amount,
	atx.amount
	order by pcg.sequence,pp.default_code
	) as p
group by
company_name,
branch_name,
phone,
time_order,
street,
street2,
city,
zip,
phone,
branch_id,
pb_id,
cate_name,
id_cate,
sequence_cate,
product,
default_code,
sum_qty,
price_unit,
sum_total,
price_subtotal_incl,
discount_amount,
before_vat,
before_vat_discount
order by  branch_id,time_order,default_code]]>
	</queryString>
