	<subDataset name="journal_1">
		<parameter name="IDS" class="java.lang.Object"/>
		<queryString>
			<![CDATA[select pp.id id,
case when is_credit_card is true
     then card_type || ': ' || overlay(credit_card_no placing 'xxxxxx' from 7 for 6)
     when (pp.journal_name = 'Cash' and pp.amount < 0 and (pp.is_return_order=false and pp.is_void_order=false))
     then 'Change CASH'
     else journal_name
end as journal_name,
(case when is_return_order=true then sum(amount)*(-1) else  sum(amount) end)as amount,
pp.is_credit_card,
case when pp.t1cc_barcode != '' and pp.t1cc_barcode is not null then concat('    Serial : ',pp.t1cc_barcode) else '' end as t1cc_barcode,
absl_id,
case
when pp.t1cc_barcode != '' and pp.t1cc_barcode is not null then 1
when is_credit_card is true then 2
when pp.journal_name = 'Cash' and pp.amount > 0 then 3
else 4
end as priority
from
(
select
absl.id as absl_id,
absl.t1cc_barcode as t1cc_barcode,
case
    when aj.redeem_type_id is not null then aj.code
    else aj.name
end as journal_name,
-- aj.name as journal_name,
aj.id as journal_id,
po.id,
po.name,
absl.amount,
absl.credit_card_no,
po.is_return_order,
po.is_void_order,
coalesce(aj.is_credit_card, false) is_credit_card,
absl.credit_card_type card_type
from pos_order as po
LEFT JOIN pos_session ps ON po.session_id = ps.id
LEFT JOIN pos_order_line pol ON po.id=pol.order_id
LEFT JOIN pos_config pf ON ps.config_id = pf.id
LEFT JOIN pos_branch pb ON pf.branch_id = pb.id
left join account_bank_statement_line absl on absl.pos_statement_id=po.id
left join account_journal aj on absl.journal_id=aj.id
where po.id in ($P!{IDS})
group by
aj.name,
aj.id,
po.id,
absl.amount,
po.is_return_order,
absl.id

)as pp
group by pp.amount, pp.is_void_order,pp.t1cc_barcode,pp.journal_name,pp.journal_id,pp.is_return_order,pp.absl_id,pp.credit_card_no,pp.is_credit_card,pp.card_type,pp.id

union

select pos.id id,'Change CASH' journal_name, case when absl.amount > 0 then 0 else absl.amount * (-1) end amount,is_credit_card,absl.t1cc_barcode,absl.id as absl_id,
	1 as priority
	   from pos_order pos
	   cross join (select count(*)
		       from pos_order po
	               left join account_bank_statement_line absl on absl.pos_statement_id=po.id
	               left join account_journal aj on absl.journal_id=aj.id
	               where po.id in ($P!{IDS})
		             and aj.is_credit_card is null
	               group by is_credit_card
	               ) as count
	   left join account_bank_statement_line absl on absl.pos_statement_id=pos.id
           left join account_journal aj on absl.journal_id=aj.id
           where case when count >= 2 then
		 pos.id in ($P!{IDS})
		 and is_credit_card is null
		 and amount < 0
		 when count < 2 then
		 pos.id in ($P!{IDS})
		 and is_credit_card is null
		 end

order by priority]]>
		</queryString>
		<field name="id" class="java.lang.Integer"/>
		<field name="journal_name" class="java.lang.String"/>
		<field name="amount" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Amount]]></fieldDescription>
		</field>
		<field name="is_credit_card" class="java.lang.Boolean">
			<fieldDescription><![CDATA[Credit Card]]></fieldDescription>
		</field>
		<field name="absl_id" class="java.lang.Integer"/>
		<field name="priority" class="java.lang.Integer">
			<fieldDescription><![CDATA[Server Priority]]></fieldDescription>
		</field>
		<field name="t1cc_barcode" class="java.lang.String"/>
	</subDataset>
	<subDataset name="ds_discount">
		<parameter name="IDS" class="java.lang.Object"/>
		<queryString>
			<![CDATA[select promotion_name,
       SUM(price_unit * qty) as order_line_subtotal
from pos_order_line
where order_id in ($P!{IDS})
      and promotion is true
      and coalesce(promotion_name, '') <> ''
      and coalesce(free_product_id, 0) = 0
group by promotion_name]]>
		</queryString>
		<field name="promotion_name" class="java.lang.String">
			<fieldDescription><![CDATA[Promotion]]></fieldDescription>
		</field>
		<field name="order_line_subtotal" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="ds_coupon">
		<parameter name="IDS" class="java.lang.Object"/>
		<parameter name="data_dir" class="java.lang.String"/>
		<queryString>
			<![CDATA[select ptp.name as coupon_name,
       (
        $P{data_dir} ||
        '/filestore/' ||
        current_database() ||
        '/' ||
        iam.store_fname
       ) as image_path,
       (
        case when pol.multi_coupon_id is null
             then prp.barcode
             else mcp.barcode
        end
       ) as barcode
from (
       select *
       from pos_order_line
       /* Parameter */
       where order_id in ($P!{IDS})
             and line_coupon = 'reward_coupon'
     ) pol
inner join product_product prp on pol.product_id = prp.id
inner join product_template ptp on prp.product_tmpl_id = ptp.id
left join ir_attachment iam on ptp.id = iam.res_id
                               and iam.res_model = 'product.template'
                               and iam.res_field = 'image_medium'
left join multi_coupon mcp on pol.multi_coupon_id = mcp.id
order by pol.id asc]]>
		</queryString>
		<field name="coupon_name" class="java.lang.String"/>
		<field name="image_path" class="java.lang.String"/>
		<field name="barcode" class="java.lang.String">
			<fieldDescription><![CDATA[Barcode]]></fieldDescription>
		</field>
	</subDataset>
	<parameter name="IDS" class="java.lang.Object">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="data_dir" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
	company_logo,
	count_line,
	coalesce(cash,0.00) as cash,
	abs(coalesce(cash_neg,0.00)) as cash_neg,
	id,
	0 as pol_id,
	pos_no,
	company_name,
	tax_id,
	phone,
	branch_name,
	date_order,
	tax_invoice,
	inv_no,
	price_unit,
	discount_percen,
	discount_amount,
	barcode,
	name_template,
	sale_name,
	is_return_order,
	vat_name,
	round((price_unit*sum(qty) ),2)  as subtotal_line,
	sum(qty) as qty,
	name_template_id,
	string_agg(to_char(pol_id,'9G999G999'), ', ') as concat_pol_id,
	subtotal  as subtotal,
	round(amount_total + change_rounding,2) as amount_total ,
	round(amount_tax,2) as amount_tax,
	round(amount_total + change_rounding,2)-round(amount_tax,2) as before_vat,
	round(amount_return,2) as amount_return,
	amount_discount as total_discount,
	change_rounding as change_rounding,
	printed_receipt,
	name_template_id,
	vip_discount_product_id,
	promotion_discount_product_id,
	past_session,
	iface_tax_included,
	(flag_best_deal || iface_line_tax_included) as iface_line_tax_included,
	pp.the_one_card_no,
	pp.the_one_card_member_name,
	product_id,
	sub_total,
	now() + INTERVAL '7 hour' as print_date
from
(
	select distinct
		po.company_logo,
		po.the_one_card_no,
		po.the_one_card_member_name,
		po.count_line,
		po.cash,
		po.cash_neg,
		po.id,
		ps.past_session,
		atx.name as vat_name,
		coalesce(pf.pos_no,'XXXXXXXXXXXXX') as pos_no,
		rpc.name as company_name,
		rpc.vat as tax_id,
		coalesce(pb.phone, '') as phone,
		coalesce(
			case when pb.branch_id = '00000'
				then ' สำนักงานใหญ่ '
				else ' สาขา '|| pb.branch_name ||' สาขาที่ '|| pb.branch_id
			end ,'' 
		)as branch_name,
		(
			case
				when ps.past_session = true then ps.start_at + interval '7 hours'
				else po.date_order + interval '7 hours'
			end
		) as date_order,
		po.tax_invoice,
		po.inv_no,
		pol.qty as qty,
		pol.price_unit as price_unit,
		pol.discount as discount_percen,
		pol.discount_amount,
		case when coalesce(pol.promotion_name,'') = ''
		     then pp.barcode
		     else fpp.barcode
		end as barcode,
		case when coalesce(pol.promotion_name,'') = ''
		     then pt.name
		     else pol.promotion_name
		end ||
		coalesce(' (' || pp.default_code || ')', '') as name_template,
		pol.id as pol_id,
		pp.id as name_template_id,
		rp.name as sale_name,
		po.is_return_order,
		pf.vip_discount_product_id,
		pf.promotion_discount_product_id,
		po.subtotal,
		po.total_discount,
		po.amount_total,
		po.amount_tax,
		po.amount_return,
		po.amount_discount,
		po.change_rounding,
		po.printed_receipt,
		po.iface_tax_included,
		case when atx.amount is null
		     then 'N'
		     else 'V'
	        end as iface_line_tax_included,
		case when coalesce(pol.promotion_name,'') = ''
		     then pol.product_id
		     else pol.free_product_id
		end product_id,
		stpol.sub_total,
		case when pt.is_best_deal_promotion is true
		     then 'B'
		     else ''
		end as flag_best_deal
	from
	(
		select po_sub.*,
			comp.company_logo,
			pol_count.count_line,
			sum_absl1.cash,
			sum_absl2.cash_neg,
			po_subtotal.subtotal,
			po_total_discount.total_discount
		from
		(
			select id,
				session_id,
				user_id,
				date_order,
				tax_invoice,
				inv_no,
				is_return_order,
				amount_total,
				amount_tax,
				amount_return,
				amount_discount,
				change_rounding,
				printed_receipt,
				iface_tax_included,
				coalesce(the_one_card_no, '') as the_one_card_no,
				coalesce(member_name, '') as the_one_card_member_name
			from pos_order
			where id in ($P!{IDS})
		) as po_sub
		cross join
		(
			SELECT value || '/web/binary/company_logo?dbname=' || current_database() as company_logo
			FROM ir_config_parameter
			WHERE key = 'report.image.url'
		) as comp
		cross join
		(
			select sum(total) as subtotal
			from
			(
				 select sum(pol.price_unit * qty) as total
				 from
				 (
					select id,
						session_id
					from pos_order
					where id in ($P!{IDS})
				 ) as po
				 left join pos_session as ps
				 on po.session_id = ps.id
				 left join pos_config as pf
				 on ps.config_id = pf.id
				 left join pos_order_line as pol
				 on po.id = pol.order_id
				 left join product_product as pp
				 on pol.product_id = pp.id
				 left join product_template as pt
				 on pp.product_tmpl_id = pt.id
				 where pt.id <> pf.vip_discount_product_id
					and pt.id <> pf.promotion_discount_product_id
					and line_coupon = 'reward_coupon'
			)as subtotal
		) as po_subtotal
		cross join
		(
			select sum(discount_amount) as total_discount
			from
			(
			  select pol.discount_amount * pol.qty as discount_amount
			  from
			  (
				select id
				from pos_order
				where id in ($P!{IDS})
			  ) as po
			  left join pos_order_line as pol
			  on po.id = pol.order_id
			)as sub_q
		) as po_total_discount
		left join
		(
			select count(id) as count_line
				,order_id
			from pos_order_line
                               where line_coupon != 'reward_coupon'
			group by order_id
		) as pol_count
		on po_sub.id = pol_count.order_id
		left join
		(
			select sum(amount) as cash
				,pos_statement_id
			from  account_bank_statement_line
			where amount > 0
			group by pos_statement_id
		) as sum_absl1
		on po_sub.id = sum_absl1.pos_statement_id
		left join
		(
			select sum(amount) as cash_neg
				,pos_statement_id
			from  account_bank_statement_line absl2
			where amount < 0
			group by pos_statement_id
		) as sum_absl2
		on po_sub.id = sum_absl2.pos_statement_id
	) as po
	left join pos_session as ps
	ON po.session_id = ps.id
	left join pos_config as pf
	ON ps.config_id = pf.id
	left join pos_branch as pb
	ON pf.branch_id = pb.id
	left join res_company as pc
	ON pb.pos_company_id = pc.id
	left join res_partner as rpc
	ON pc.partner_id = rpc.id
	left join pos_order_line as pol
	on po.id = pol.order_id
	left join product_product as pp
	on pol.product_id = pp.id
	left join product_product as fpp
	on pol.free_product_id = fpp.id
	left join product_template as pt
	on pp.product_tmpl_id = pt.id
	left JOIN product_category as pcg
	ON pt.categ_id = pcg.id
	left join res_users as ru
	on ru.id = po.user_id
	left join res_partner as rp
	on rp.id = ru.partner_id
	left join account_tax_pos_order_line_rel as atp
	on atp.pos_order_line_id = pol.id
	left join account_tax as atx
	on atp.account_tax_id = atx.id
	left join pos_promotion pmt on pol.promotion_id = pmt.id
	cross join(
		   SELECT SUM(price_unit * qty) as sub_total
		   FROM pos_order_line
		   where order_id in ($P!{IDS})
			 and promotion is false
		  ) as stpol
        where line_coupon != 'reward_coupon'
) as pp
where product_id > 0
group by
past_session,
company_logo,
count_line,
cash,
cash_neg,
id,
pos_no,
company_name,
tax_id,
phone,
branch_name,
date_order,
tax_invoice,
inv_no,
price_unit,
discount_percen,
discount_amount,
barcode,
name_template,
vat_name,
sale_name,
is_return_order,
before_vat,
name_template_id,
subtotal,
amount_total,
amount_tax,
amount_return,
total_discount,
printed_receipt,
name_template_id,
vip_discount_product_id,
promotion_discount_product_id,
iface_tax_included,
iface_line_tax_included,
amount_discount,
change_rounding,
pp.the_one_card_no,
pp.the_one_card_member_name,
product_id,
sub_total,
flag_best_deal
order by product_id,
         qty desc]]>
	</queryString>
