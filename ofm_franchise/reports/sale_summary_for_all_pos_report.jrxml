<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sale_summary_for_all_pos_report" language="groovy" pageWidth="612" pageHeight="792" columnWidth="572" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.6105100000000008"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="559"/>
	<parameter name="company_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[company_id]]></defaultValueExpression>
	</parameter>
	<parameter name="start_date" class="java.lang.String">
		<parameterDescription><![CDATA[start_date]]></parameterDescription>
		<defaultValueExpression><![CDATA[start_date]]></defaultValueExpression>
	</parameter>
	<parameter name="end_date" class="java.lang.String">
		<defaultValueExpression><![CDATA[end_date]]></defaultValueExpression>
	</parameter>
	<parameter name="branch_id" class="java.lang.String">
		<parameterDescription><![CDATA[branch_id]]></parameterDescription>
		<defaultValueExpression><![CDATA[branch_id]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	(now()+interval '7 hour')::date as print_date,
	coalesce(pos_vat.sale_vat_total,0) as pos_vat_total,
	coalesce(pos_vat_discount.sale_vat_disccount_total,0) as pos_vat_disccount_total,
	coalesce(pos_no_vat.sale_no_vat_total,0) as pos_no_vat_total,
	coalesce(pos_no_vat_discount.sale_no_vat_disccount_total,0) as pos_no_vat_disccount_total,
	coalesce(sale_vat.sale_order_vat,0) as sale_order_vat,
	coalesce(sale_discount_vat.sale_discount_vat,0) as sale_discount_vat,
	coalesce(deposit_vat.sale_deposit_total,0) as sale_deposit_vat_total,
	coalesce(clear_deposit_vat.invoice_clear_deposit,0) as invoice_vat_clear_deposit,
	coalesce(invoice_return_vat.return_amount_total,0) as invoice_vat_return_total,
	coalesce(pos_return_vat.pos_return_total,0) as pos_vat_return_total,
	coalesce(clear_deposit_refund_vat.clear_deposit_total,0) as clear_vat_deposit_total,
	coalesce(sale_no_vat.sale_order_vat,0) as sale_order_no_vat,
	coalesce(sale_discount_no_vat.sale_discount_no_vat,0) as sale_discount_no_vat,
	coalesce(deposit_no_vat.sale_deposit_total,0) as sale_deposit_no_vat_total,
	coalesce(clear_deposit_no_vat.invoice_clear_deposit,0) as invoice_no_vat_clear_deposit,
	coalesce(invoice_return_no_vat.return_amount_total,0) as invoice_no_vat_return_total,
	coalesce(pos_return_no_vat.pos_return_total,0) as pos_no_vat_return_total,
	coalesce(clear_deposit_refund_no_vat.clear_deposit_total,0) as clear_no_vat_deposit_total,
	coalesce(receive_total.cash_total,0) as cash_total,
	coalesce(receive_total.credit_card_total,0) as credit_card_total,
	coalesce(receive_total.cheque,0) as cheque_total,
	coalesce(receive_total.t1cc,0) as t1cc,
	coalesce(sale_credit_vat.sale_total,0) as sale_credit_vat,
	coalesce(sale_credit_return_vat.return_total,0) as sale_credit_return_vat,
	coalesce(invoice_rounding.invoice_rounding,0) as invoice_rounding,
	coalesce(invoice_return_rounding.invoice_return_rounding,0) as invoice_return_rounding,
	coalesce(deposit_rouding.deposit_rouding,0) as deposit_rouding,
	coalesce(pos_rounding.pos_rounding,0) as pos_rounding,
	coalesce(pos_return_rounding.pos_return_rounding,0) as pos_return_rounding,
	branch.company_name,
	branch.branch_id,
	branch.name as branch_name,
	branch.tax_id_company,
	branch.name,
	branch.adddress,
	branch.adddress2,
	branch.city,
	branch.zip,
	branch.company_logo
from
(
	select sum(pol.price_subtotal_incl) as sale_vat_total
			from pos_order_line pol
				inner join pos_order po on pol.order_id = po.id
			where
				(po.is_return_order is false
				or po.is_void_order is True)
				and po.amount_tax <> 0
				and pol.promotion_id is null
				and DATE(po.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
				and po.branch_id in ($P!{branch_id})
) as pos_vat,
(
	select sum(abs(pol.price_subtotal)) as sale_vat_disccount_total
			from pos_order_line pol
				inner join pos_order po on pol.order_id = po.id
			where
				(po.is_return_order is false
				or po.is_void_order is True)
				and po.amount_tax <> 0
				and pol.promotion_id is not null
				and DATE(po.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
				and po.branch_id in ($P!{branch_id})
) as pos_vat_discount,
(
	select sum(pol.price_subtotal_incl) as sale_no_vat_total
			from pos_order_line pol
				inner join pos_order po on pol.order_id = po.id
			where
				(po.is_return_order is false
				or po.is_void_order is True)
				and po.amount_tax = 0
				and pol.promotion_id is null
				and DATE(po.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	--			and pol.company_id = 1
				and po.branch_id in ($P!{branch_id})
) as pos_no_vat,
(
	select sum(abs(pol.price_subtotal_incl)) as sale_no_vat_disccount_total
			from pos_order_line pol
				inner join pos_order po on pol.order_id = po.id
			where
				(po.is_return_order is false
				or po.is_void_order is True)
				and po.amount_tax = 0
				and pol.promotion_id is not null
				and DATE(po.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	--			and pol.company_id = 1
				and po.branch_id in ($P!{branch_id})
) as pos_no_vat_discount,
(
	select sum(sol.price_subtotal+sol.price_tax) as sale_order_vat
		from sale_order_line sol
			inner join sale_order so on sol.order_id = so.id
		where so.state in ('sale','done')
		and sol.promotion is false
		and sol.price_tax > 0
		and so.sale_payment_type <> 'deposit'
		and so.branch_id in ($P!{branch_id})
		and DATE(so.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as sale_vat,
(
	select sum(abs(sol.price_subtotal+sol.price_tax)) as sale_discount_vat
		from sale_order_line sol
			inner join sale_order so on sol.order_id = so.id
		where so.state in ('sale','done')
		--	and so.company_id = 1
			and so.branch_id in ($P!{branch_id})
		and DATE(so.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
		and sol.price_tax < 0
		and sol.price_subtotal < 0
) as sale_discount_vat,
(
	select sum(ad.amount_untaxed+ad.amount_tax) as sale_deposit_total
	from account_deposit ad
		inner join sale_order so on ad.sale_id = so.id
	where
	so.state in ('sale','done','cancel')
	and ad.amount_tax <> 0
          and so.sale_payment_type = 'deposit'
	and so.branch_id in ($P!{branch_id})
	and DATE(ad.date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as deposit_vat,
(
	select sum(ai.amount_deposit+ai.amount_tax_deposit) as invoice_clear_deposit
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done')
	and so.sale_payment_type = 'deposit'
	and ai.state in ('open','paid')
	and ai.type = 'out_invoice'
	and ai.amount_tax <> 0
	--and ai.company_id = 1
	and ai.branch_id in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as clear_deposit_vat,
(
	select sum(ai.amount_total) as return_amount_total
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done','cancel')
	and so.sale_payment_type = 'deposit'
	and ai.type = 'out_refund'
	and ai.state = 'paid'
	and ai.amount_deposit = 0
	and ai.amount_tax < 0
--	and ai.company_id = 1
	and ai.branch_id in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as invoice_return_vat,
(
	select
	sum(abs(pol.price_subtotal_incl)) as pos_return_total
	from pos_order_line pol
		inner join pos_order po on pol.order_id = po.id
	where
		po.is_return_order is true
		and pol.promotion_id is null
		and po.amount_tax < 0
		and DATE(po.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	--	and pol.company_id = 1
		and po.branch_id in ($P!{branch_id})
) as pos_return_vat,
(
	select sum(abs(round(ai.amount_total_signed,2))) as clear_deposit_total
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done','cancel')
	and so.sale_payment_type = 'deposit'
	and ai.type = 'out_refund'
	and ai.state = 'paid'
	and ai.parent_invoice_id is null
	and ai.amount_deposit > 0
	and ai.amount_tax <> 0
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ai.branch_id in ($P!{branch_id})
) clear_deposit_refund_vat,
(
	select sum(sol.price_subtotal) as sale_order_vat
		from sale_order_line sol
			inner join sale_order so on sol.order_id = so.id
		where so.state in ('sale','done')
			and so.sale_payment_type <> 'deposit'
			and so.branch_id  in ($P!{branch_id})
		and DATE(so.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
		and sol.promotion is false
		and sol.price_tax = 0
) as sale_no_vat,
(
	select sum(abs(sol.price_subtotal)) as sale_discount_no_vat
		from sale_order_line sol
			inner join sale_order so on sol.order_id = so.id
		where so.state in ('sale','done')
			and so.branch_id  in ($P!{branch_id})
		and DATE(so.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
		and sol.promotion is True
		and sol.price_tax = 0
		and sol.price_subtotal < 0
) as sale_discount_no_vat,
(
	select sum(ad.amount_untaxed+ad.amount_tax) as sale_deposit_total
	from account_deposit ad
		inner join sale_order so on ad.sale_id = so.id
	where
	so.state in ('sale','done','cancel')
	and ad.amount_tax = 0
	and so.sale_payment_type = 'deposit'
	and so.branch_id  in ($P!{branch_id})
	and DATE(ad.date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as deposit_no_vat,
(
	select sum(ai.amount_deposit) as invoice_clear_deposit
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done')
	and so.sale_payment_type = 'deposit'
	and ai.state in ('open','paid')
	and ai.type = 'out_invoice'
	and ai.amount_tax = 0
	and ai.branch_id  in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as clear_deposit_no_vat,
(
	select sum(ai.amount_total) as return_amount_total
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done','cancel')
	and so.sale_payment_type = 'deposit'
	and ai.type = 'out_refund'
	and ai.state = 'paid'
	and ai.amount_deposit = 0
	and ai.amount_tax = 0
	and ai.branch_id in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as invoice_return_no_vat,
(
	select
	sum(abs(pol.price_subtotal_incl)) as pos_return_total
	from pos_order_line pol
		inner join pos_order po on pol.order_id = po.id
	where
		po.is_return_order is true
		and pol.promotion_id is null
		and po.amount_tax = 0
		and DATE(po.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
		and po.branch_id  in ($P!{branch_id})
) as pos_return_no_vat,
(
	select sum(abs(round(ai.amount_total_signed,2))) as clear_deposit_total
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done','cancel')
	and so.sale_payment_type = 'deposit'
	and ai.type = 'out_refund'
	and ai.state = 'paid'
	and ai.parent_invoice_id is null
	and ai.amount_deposit > 0
	and ai.amount_tax = 0
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ai.branch_id  in ($P!{branch_id})
) clear_deposit_refund_no_vat,
(
select
  coalesce(coalesce(pos_cash.cash_total,0) + coalesce(sale_order_cash.cash_total,0) + coalesce(receipt_cash.cash_total,0),0) as cash_total,
  coalesce(coalesce(pos_credit_card.cash_total,0) + coalesce(sale_order_credit_card.cash_total,0) + coalesce(receipt_credit_card.cash_total,0),0) as credit_card_total,
  0.00 as cheque,
  0.00 as t1cc
from
(
select
	sum(absl.amount) as cash_total
	from
	account_bank_statement_line absl
			inner join pos_order pos on absl.pos_statement_id = pos.id
			inner join account_journal aj on absl.journal_id = aj.id
			where
			aj.type = 'cash'
			and (pos.is_return_order is false or pos.is_void_order is True)
			and aj.is_credit_card is false
			and DATE(pos.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
			and pos.branch_id  in ($P!{branch_id})
) as pos_cash,
(
select
	sum(adpt_line.paid_total) as cash_total
from account_deposit_payment_line as adpt_line
	inner join account_journal as aj on adpt_line.journal_id = aj.id
	inner join account_deposit ad on adpt_line.payment_id = ad.id
	inner join sale_order so on ad.sale_id = so.id
	where
	aj.type = 'cash'
	and aj.is_credit_card is false
	and ad.invoice_id is null
	and ad.state = 'paid'
	and DATE(ad.date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ad.branch_id  in ($P!{branch_id})
) as sale_order_cash,
(
select sum(apl.paid_total) as cash_total
from account_payment_line apl
inner join account_payment ap on apl.payment_id = ap.id
inner join account_payment_method_multi apm on apl.payment_method_id = apm.id
inner join account_journal aj on apm.journal_id = aj.id
where
	aj.type = 'cash'
	and aj.is_credit_card is false
	and ap.state = 'posted'
	and ap.type_payment_to_customer = 'inbound'
	and DATE(ap.payment_date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ap.branch_id  in ($P!{branch_id})
) as receipt_cash,
(
select
	sum(absl.amount) as cash_total
	from
	account_bank_statement_line absl
			inner join pos_order pos on absl.pos_statement_id = pos.id
			inner join account_journal aj on absl.journal_id = aj.id
			where
			pos.is_return_order is false
			and aj.is_credit_card is true
			and DATE(pos.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
			and pos.branch_id  in ($P!{branch_id})
) as pos_credit_card,
(
select
	sum(adpt_line.paid_total) as cash_total
from account_deposit_payment_line as adpt_line
	inner join account_journal as aj on adpt_line.journal_id = aj.id
	inner join account_deposit ad on adpt_line.payment_id = ad.id
	inner join sale_order so on ad.sale_id = so.id
	where
	aj.is_credit_card is true
	and ad.invoice_id is null
	and ad.state = 'paid'
	and DATE(ad.date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ad.branch_id in ($P!{branch_id})
) as sale_order_credit_card,
(
select sum(apl.paid_total) as cash_total
from account_payment_line apl
inner join account_payment ap on apl.payment_id = ap.id
inner join account_payment_method_multi apm on apl.payment_method_id = apm.id
inner join account_journal aj on apm.journal_id = aj.id
where
	 aj.is_credit_card is true
	and ap.state = 'posted'
	and ap.type_payment_to_customer = 'inbound'
	and DATE(ap.payment_date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ap.branch_id in ($P!{branch_id})
) as receipt_credit_card
) receive_total,
(
	select sum(sol.price_subtotal+sol.price_tax) as sale_total
		from sale_order_line sol
			inner join sale_order so on sol.order_id = so.id
		where so.state in ('sale','done')
		and sol.promotion is false
		and sol.price_tax > 0
		and so.sale_payment_type = 'credit'
			and so.branch_id in ($P!{branch_id})
		and DATE(so.date_order+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as sale_credit_vat,
(
	select sum(ai.amount_total) as return_total
	from account_invoice ai
		inner join account_invoice ai_ref on ai.parent_invoice_id = ai_ref.id
		inner join sale_order so on ai_ref.so_id = so.id
	where
	so.state in ('sale','done')
	and so.sale_payment_type = 'credit'
	and ai.type = 'out_refund'
	and ai.state = 'paid'
	and ai.amount_deposit = 0
	and ai.amount_tax > 0
	and ai.branch_id in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as sale_credit_return_vat,
(
select sum(ai.change_rounding) as invoice_rounding
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done')
	and so.sale_payment_type != 'credit'
	and ai.state in ('open','paid')
	and ai.type = 'out_invoice'
	and ai.branch_id in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as invoice_rounding,
(
select sum(po.change_rounding) as pos_rounding
	from pos_order po
		inner join pos_session ps on po.session_id = ps.id
	where
	po.state in ('paid','done','invoiced')
	and po.branch_id in ($P!{branch_id})
	and po.is_return_order = False
	and DATE(ps.stop_at+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as pos_rounding,
(
select sum(ai.change_rounding) as invoice_return_rounding
	from account_invoice ai
		inner join sale_order so on ai.so_id = so.id
	where
	so.state in ('sale','done')
	and so.sale_payment_type != 'credit'
	and ai.state in ('open','paid')
	and ai.type = 'out_refund'
	and ai.branch_id in ($P!{branch_id})
	and DATE(ai.date_invoice+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as invoice_return_rounding,
(
select sum(po.change_rounding) as pos_return_rounding
	from pos_order po
		inner join pos_session ps on po.session_id = ps.id
	where
	po.state in ('paid','done','invoiced')
	and po.branch_id in ($P!{branch_id})
	and po.is_return_order = True
	and DATE(ps.stop_at+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
) as pos_return_rounding,
(
select sum(ad.change_rounding) as deposit_rouding
	from account_deposit ad
		inner join sale_order so on ad.sale_id = so.id
	where
	so.state in ('sale','done','cancel')
	and so.sale_payment_type != 'credit'
	and DATE(ad.date+interval '7 hours') BETWEEN $P{start_date}::date AND  $P{end_date}::date
	and ad.branch_id in ($P!{branch_id})
) as deposit_rouding,
(
select
pc.name as company_name,
rp.vat AS tax_id_company,
	pb.branch_name as name,
	pb.sequence AS sequence,
	pb.branch_id as branch_id,
	pb.street as adddress,
	pb.street2 as adddress2,
	pb.city as city,
	pb.zip as zip,
	COALESCE(pb.phone, '') as phone,(SELECT value || '/web/binary/company_logo?dbname=' || current_database()
		FROM ir_config_parameter
		WHERE key = 'web.base.url') as company_logo
from
    pos_branch pb
	LEFT JOIN res_company pc ON pb.pos_company_id = pc.id
	LEFT JOIN res_partner rpc ON pc.partner_id = rpc.id
	LEFT JOIN province pro_com ON rpc.province_id = pro_com.id
	LEFT JOIN res_country_state rcs ON pb.state_id=rcs.id
	LEFT JOIN res_partner rp ON rp.id=pc.partner_id
	LEFT JOIN province pro_cus ON rp.province_id = pro_cus.id
	LEFT JOIN res_country_state rcs2 ON rp.state_id=rcs2.id
	where pb.id in ($P!{branch_id})
) as branch]]>
	</queryString>
	<field name="print_date" class="java.sql.Date"/>
	<field name="pos_vat_total" class="java.math.BigDecimal"/>
	<field name="pos_vat_disccount_total" class="java.math.BigDecimal"/>
	<field name="pos_no_vat_total" class="java.math.BigDecimal"/>
	<field name="pos_no_vat_disccount_total" class="java.math.BigDecimal"/>
	<field name="sale_order_vat" class="java.math.BigDecimal"/>
	<field name="sale_discount_vat" class="java.math.BigDecimal"/>
	<field name="sale_deposit_vat_total" class="java.lang.Double"/>
	<field name="invoice_vat_clear_deposit" class="java.math.BigDecimal"/>
