			<variableExpression><![CDATA[$V{sum_qty%}]]></variableExpression>
		</variable>
		<variable name="เปอเซนมูลค่าทั้งหมด" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{sum_price%}]]></variableExpression>
