	<queryString>
		<![CDATA[with
company_logo as (
    SELECT value || '/web/binary/company_logo?dbname=' ||
    current_database() AS company_logo
    FROM ir_config_parameter
    WHERE key = 'report.image.url'
    ),
company_info as (
    select
    (now() + INTERVAL '0 hour')::date AS print_date,
    (now() + INTERVAL '0 hour')::time AS print_time,
    pos_b.pos_company_id company_id,
    pos_b.id as branch_id,
    pos_b.id pos_branch_id,
    pos_b.branch_id branch_id_str,
    rcp.name ||
    case when pos_b.branch_id = '00000'
    then ' สำนักงานใหญ่ '
    else ' สาขา '|| pos_b.branch_name ||' สาขาที่ '|| pos_b.branch_id end head_company_name,
    coalesce(pos_b.street,'') || ' ' ||
    coalesce(pos_b.moo,'') || ' ' ||
    coalesce(pos_b.alley,'') || ' ' ||
    coalesce(pos_b.street2,'') || ' ' ||
    coalesce(case when pv.code = '10'
        then 'แขวง'
        else 'ตำบล'
        end || tmb.name,'') || ' ' ||
    coalesce(case when pv.code = '10'
        then 'เขต'
        else 'อำเภอ'
        end || amp.name,'') || ' ' ||
    coalesce(
        case when pv.name like '%กรุงเทพ%'
        then pv.name
        else ('จังหวัด' || pv.name)
        end
        , ''
        ) || ' ' ||
    coalesce(zp.name,'') ||
    coalesce(' โทร.' ||pos_b.phone,'') ||
    '<br> เลขประจำตัวผู้เสียภาษีอากร: '
    || rpn.vat as head
    from (select *
        from pos_branch pos_b
        where (pos_b.id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '')
        ) pos_b
    inner join (select *
        from res_company
        where (res_company.id = $P{company_id}::integer or COALESCE($P{company_id}, '') = '')
        )rcp on rcp.id = pos_b.pos_company_id
    inner join res_partner rpn on rpn.id = rcp.partner_id
    INNER JOIN province pv ON pos_b.province_id = pv.id
    INNER JOIN amphur amp ON pos_b.amphur_id = amp.id
    INNER JOIN tambon tmb ON pos_b.tambon_id = tmb.id
    INNER JOIN zip zp ON pos_b.zip_id = zp.id
    ),
-----------------------------------------------------------------
tb_inv as (
   select id,
   date_invoice as date_invoice,
   company_id,
   branch_id,
   amount_total,
   amount_tax
   from account_invoice cn
   where
   cn.so_id is not null
   and cn.type = 'out_refund'
   and cn.state in ('open', 'paid')
   and (cn.date_invoice + INTERVAL '0 hour') between $P{start_date}::date and $P{end_date}::date
   and (cn.company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = '')
   and (cn.branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '')
   ),
tb_date_order as (
    select id,
    date_invoice as date_invoice,
    company_id,
    branch_id,
    amount_total,
    amount_tax
  from tb_inv

  UNION

  select 0 as id,
  grs.date_invoice,
  pos_company_id,
  branch_id,
  0 as amount_total,
  0 as amount_tax
  from (
   select generate_series(
      ($P{start_date}::DATE),
      ($P{end_date}::DATE),
      interval '1 day'
      )::date as date_invoice,
   generate_series(
      ($P{start_date}::DATE),
      ($P{end_date}::DATE),
      interval '1 day'
      )::date::varchar || id::varchar as join_field,
   id as branch_id,
   pos_company_id
   from pos_branch
   where 
   (pos_company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = '')
   and (id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '')
   ) grs
  left join (
     select date_invoice::varchar || branch_id::varchar as join_field
     from tb_inv
     group by date_invoice, branch_id
     ) inv on inv.join_field = grs.join_field
  where coalesce(inv.join_field,'') = ''
  ),

------------------------------------------------------------------------------
tb_cn as (
    select
    cn.id as cn_id,
    '1'::text as return_receipt_no,
    tdo.date_invoice,
    tdo.branch_id,
    tdo.company_id,
    sum(coalesce(cn.amount_total_signed,0)) as amount_total,
    sum(coalesce(cn.amount_untaxed,0)) as amount_untaxed,
    sum(coalesce(cn.amount_tax,0)) as amount_tax

    from tb_date_order tdo 
    left join account_invoice cn on cn.id = tdo.id
    group by tdo.date_invoice,tdo.branch_id,cn.id,tdo.company_id
    ),
tb_cn_line as (
    select 
    tb_cn.date_invoice,
    tb_cn.branch_id,
    coalesce(sum(( cn_line.price_unit * cn_line.quantity )) ,0) as line_total_amount,
    coalesce(sum(case
      when atx.id is null
      then cn_line.price_subtotal
    end),0) as price_non_vat,
    coalesce(sum(case
      when atx.id is not null
      then cn_line.price_subtotal
    end),0) as price_vat,

    coalesce(sum(case when atx.amount is null
    then 0
    else (cn_line.price_unit * cn_line.quantity) - ((cn_line.price_unit * cn_line.quantity) * 100) / (atx.amount + 100)
  end), 0) AS amount_tax,
  
    atx.id as tax_id
    
    from tb_cn as tb_cn 
  left join account_invoice_line as cn_line on tb_cn.cn_id = cn_line.invoice_id
  left join account_invoice_line_tax ail_tax on ail_tax.invoice_line_id = cn_line.id
  left join account_tax atx on atx.id = ail_tax.tax_id
    group by atx.id,tb_cn.date_invoice,tb_cn.branch_id
    )

select * from 

(
  select 
      (tb_cn.date_invoice + INTERVAL '0 hour') ::date as date_invoice,
      tb_cn.branch_id,
      tb_cn.company_id,
      abs(coalesce(tb_cn.amount_total ,0)) as line_total_amount,
      coalesce(tb_cn_line.price_non_vat ,0) as price_non_vat,
      coalesce(tb_cn.amount_untaxed ,0) - coalesce(tb_cn_line.price_non_vat ,0) as price_vat,
      coalesce(tb_cn.amount_tax ,0) as amount_tax
    
  from (
    select 
      date_invoice,
      branch_id,
      company_id,
      sum(coalesce(amount_total ,0)) as amount_total,
      sum(coalesce(amount_untaxed ,0)) as amount_untaxed,
      sum(coalesce(amount_tax ,0)) as amount_tax
    from tb_cn
     group by date_invoice, branch_id, company_id
  )tb_cn
  left join (
    select date_invoice,
      branch_id,
      sum(coalesce(line_total_amount ,0)) as amount_total,
      sum(coalesce(price_non_vat ,0)) as price_non_vat,
      sum(coalesce(price_vat ,0)) as price_vat,
      sum(coalesce(amount_tax ,0)) as amount_tax
    from tb_cn_line
    group by date_invoice, branch_id 
  )tb_cn_line on tb_cn.date_invoice = tb_cn_line.date_invoice and tb_cn.branch_id = tb_cn_line.branch_id
  group by tb_cn.date_invoice, tb_cn.amount_total, tb_cn_line.price_non_vat, tb_cn_line.price_vat, tb_cn.amount_tax, tb_cn.amount_untaxed,
    tb_cn.branch_id, tb_cn.company_id
)temp

inner join company_info com_info on com_info.company_id = temp.company_id and com_info.branch_id = temp.branch_id
cross join company_logo com_logo


order by com_info.branch_id, com_info.branch_id, date_invoice]]>
