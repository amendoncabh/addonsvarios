		<variableExpression><![CDATA[$V{COLUMN_COUNT}]]></variableExpression>
	</variable>
	<variable name="count_line" class="java.lang.Integer" resetType="Group" resetGroup="account_id" calculation="Count">
		<variableExpression><![CDATA[$V{COLUMN_NUMBER}]]></variableExpression>
	</variable>
	<variable name="sum_amt" class="java.lang.Float" resetType="Group" resetGroup="account_id" calculation="Sum">
		<variableExpression><![CDATA[$F{inv_line_amount}]]></variableExpression>
	</variable>
	<variable name="bahttext" class="java.lang.String">
		<variableExpression><![CDATA[org.numbertext.Numbertext.moneytext(new DecimalFormat("#0.00").format(($F{amount_total}=='0')?$F{amount_total}:$V{sum_amt}),"THB","th_TH")]]></variableExpression>
	</variable>
	<variable name="sum_qty" class="java.math.BigDecimal" resetType="Group" resetGroup="account_id" calculation="Sum">
		<variableExpression><![CDATA[$F{inv_line_quantity}]]></variableExpression>
	</variable>
	<variable name="amount_untaxed" class="java.math.BigDecimal" resetType="Group" resetGroup="account_id" calculation="Sum">
		<variableExpression><![CDATA[($F{inv_line_tax}=='N')?$F{inv_line_amount_subtotal}:0]]></variableExpression>
