			<variableExpression><![CDATA[(100*$F{price_line})/$F{price_all}]]></variableExpression>
		</variable>
		<variable name="sum_qty%" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{qty%}]]></variableExpression>
		</variable>
