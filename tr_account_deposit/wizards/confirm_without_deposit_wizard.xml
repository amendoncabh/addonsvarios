<?xml version="1.0"?>
<odoo>
    <data>
        <record id="confirm_without_deposit_form_wizard" model="ir.ui.view">
            <field name="name">>confirm.without.deposit.form.wizard</field>
            <field name="model">confirm.without.deposit.wizard</field>
            <field name="arch" type="xml">
                <form string="Confirm Without Deposit">
                    <p>Are you confirm sale without deposit?</p>
                    <footer>
                        <button name="action_confirm"
                                string="Confirm"
                                type="object"
                                class="oe_highlight"/>
                        or
                        <button string="Cancel"
                                class="oe_link"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
