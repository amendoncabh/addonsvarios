	<queryString>
		<![CDATA[with table_check_head_deposit as (
	select ai.id from
	account_invoice ai
	inner join account_invoice ai_init on ai_init.id = ai.parent_invoice_id
  	inner join sale_order so on so.id = ai_init.so_id
	inner join account_deposit adp on adp.sale_id = so.id
	where adp.state not in ('draft', 'cancel')
	and ai.id in ($P!{IDS})
	),
table_check_have_so_credit_term as (
  select ai.id from
  account_invoice ai
  inner join account_invoice ai_init on ai_init.id = ai.parent_invoice_id
  inner join sale_order so on so.id = ai_init.so_id
  and so.sale_payment_type = 'credit'
  where ai.id in ($P!{IDS})
  )
select
account_id,
company_name,
company_addr,
doc_no,
date_invoice,
parent_invoice_id,
(
	case
	when coalesce((select id from table_check_head_deposit), 0) = 0
	and parent_invoice_id is not null
	then 'ใบลดหนี้ (Credit Note)'
	when pos_id is not null
	or deposit_id is null
	and parent_invoice_id is not null
	then 'ใบลดหนี้ (Credit Note)'
	when deposit_id is not null
	or parent_invoice_id is null
	then 'ใบลดหนี้ / มัดจำ (Credit Note / Deposit)'
	end
	) as form_name,
change_cn_detail,
(
	case when so_id is null
	then pos_customer_name
	else so_customer_name
	end
	) as customer_name,
customer_addr,
inv_line_tax,
inv_line_product_barcode,
inv_line_product_name,
sum(inv_line_quantity) as inv_line_quantity,
inv_line_unit_price,
sum(inv_line_amount) as inv_line_amount,
sum(inv_line_amount_subtotal) as inv_line_amount_subtotal,
amount_total,
amount_tax,
tax,
doc_type,
page,
receipt_no,
round_adjust,
pos_id,
amount_parent_inv,
true_amount,
abs(amount_parent_inv - true_amount) as difference_amount,
discount_line_non_vat,
discount_line_vat,
parent_name,
coalesce(parent_no,deposit_name) as parent_no,
return_reason,
is_void_order,
is_return_order,
deposit_id,
(
	case when pos_id is not null
	then True
	else False
	end
	) as is_show_receipt_no,
(
	case when (pos_id is not null
	or deposit_id is not null
	or (deposit_id is null
		and invoice_state = 'paid'
		)
	) and coalesce((select id from table_check_have_so_credit_term), 0) = 0
	then True
	else False
	end
	) as is_show_payment,
(
	case when deposit_id is null
	and pos_id is null
	then account_id
	else null
	end
	) as non_deposit_inv_id

from (
	select aci.id as account_id,
	(
		rep_com.name  ||
		coalesce(
			case when rep_branch.branch_id = '00000'
			then 'สำนักงานใหญ่'
			else ' สาขา ' || rep_branch.name
			end, ''
			) ||
		coalesce('สาขาที่ ' || rep_branch.branch_id, '')
		) as company_name,
	(
		coalesce(rep_branch.street,'') || ' ' ||
		coalesce(rep_branch.moo,'') || ' ' ||
		coalesce(rep_branch.alley,'') || ' ' ||
		coalesce(rep_branch.street2,'') || '<br>' ||
		coalesce(
			case when pv.code = '10'
			then 'แขวง'
			else 'ตำบล'
			end || tmb.name, ''
			) || ' ' ||
		coalesce(
			case when pv.code = '10'
			then 'เขต'
			else 'อำเภอ'
			end || amp.name, ''
			) || ' ' ||
		coalesce(
			case when pv.name like '%กรุงเทพ%'
			then pv.name
			else ('จังหวัด' || pv.name)
			end, ''
			) || ' ' ||
		coalesce(zp.name, '')
		|| '<br>โทร.' || coalesce(rep_branch.phone,'') ||
		coalesce(' เลขประจำตัวผู้เสียภาษีอากร: ' || rep_com.vat, rep_com.number, '')
		) as company_addr,
	aci.number as doc_no,
	aci.parent_invoice_id as parent_invoice_id,
	(
		TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'dd') || ' ' ||
		case when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '01'
		then 'มกราคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '02'
		then 'กุมภาพันธ์'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '03'
		then 'มีนาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '04'
		then 'เมษายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '05'
		then 'พฤษภาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '06'
		then 'มิถุนายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '07'
		then 'กรกฏาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '08'
		then 'สิงหาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '09'
		then 'กันยายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '10'
		then 'ตุลาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '11'
		then 'พฤศจิกายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '12'
		then 'ธันวาคม'
		end || ' ' ||
		TO_CHAR(DATE(aci.date_invoice + interval '543 years'), 'yyyy')
		) as date_invoice,
	(
		'*** ยกเลิกใบกำกับภาษีฉบับเดิมเลขที่ ' ||
		origin_inv.number ||
		' *** ' ||
		aci.note
		) as change_cn_detail,
	(
		coalesce(rep_so_cus.name,'') || ' ' ||
		coalesce(
			case when rep_so_cus.shop_id = '00000'
			then 'สำนักงานใหญ่'
			else coalesce(' สาขาที่ ' || rep_so_cus.shop_id, '')
			end, ''
			) || ' เลขประจำตัวผู้เสียภาษีอากร ' ||
		coalesce(rep_so_cus.vat, rep_so_cus.number)
		) as so_customer_name,
	(
		coalesce(rep_cus.name,'') || ' ' ||
		coalesce(
			case when rep_cus.shop_id = '00000'
			then 'สำนักงานใหญ่'
			else coalesce(' สาขาที่ ' || rep_cus.shop_id, '')
			end, ''
			) || ' เลขประจำตัวผู้เสียภาษีอากร ' ||
		coalesce(rep_cus.vat, rep_cus.number)
		) as pos_customer_name,
	aci.address_inv as customer_addr,
	case when t.amount is null
	then 'N'
	else 'V'
	end as inv_line_tax,
	prp.id as inv_line_product_id,
	prp.barcode as inv_line_product_barcode,
	prt.name as inv_line_product_name,
	abs(ail.quantity) as inv_line_quantity,
	ail.price_unit as inv_line_unit_price,
	abs(ail.amount_subtotal) as inv_line_amount,
	abs(ail.amount_subtotal) as inv_line_amount_subtotal,
	abs(aci.amount_total) as amount_total,
	abs(aci.amount_tax) as amount_tax,
	abs(coalesce(t.amount, 0)) as tax,
	coalesce(cal_dis.discount_non_taxed, 0) as discount_line_non_vat,
	coalesce(cal_dis.discount_taxed, 0) as discount_line_vat,
	aci.doc_type,
	aci.page,
	po.inv_no as receipt_no,
	coalesce(po.change_rounding, 0) as round_adjust,
	po.id as pos_id,
	abs(parent_inv.amount_total - parent_inv.amount_tax) as amount_parent_inv,
	abs(
		abs(
			parent_inv.amount_total - parent_inv.amount_tax
			) -
		abs(
			aci.amount_total - aci.amount_tax
			)
		) as true_amount,
	(
		case when aci.pos_id is not null
		then 'อ้างอิงใบกำกับภาษีอย่างย่อเลขที่ :'
		else 'อ้างอิงใบกำกับภาษีเลขที่ :'
		end
		) as parent_name,
	(
		case when aci.pos_id is not null
		then parent_pod.inv_no
		else parent_aci.number
		end
		) as parent_no,
	rtr.name as return_reason,
	po.is_void_order as is_void_order,
	po.is_return_order as is_return_order,
	adai.account_deposit_id as deposit_id,
	adpt.name as deposit_name,
	aci.state as invoice_state,
	aci.so_id
	from (
		select *,
		case when print_time <= 1
		then 'ต้นฉบับ'
		else 'สำเนา'
		end as doc_type, 1 as page
		from account_invoice
		/* Parameter */
		where id in ($P!{IDS})
		and state not in ('draft', 'cancel')
		union all
		select *,
		'สำเนา' as doc_type, 2 as page
		from account_invoice
		/* Parameter */
		where id in ($P!{IDS})
		and state not in ('draft', 'cancel')
		and print_time <= 1
		) aci
	left join pos_order po on aci.pos_id = po.id
	left join pos_order parent_pod on po.return_order_id = parent_pod.id
	left join sale_order sod on aci.so_id = sod.id
	left join res_partner rep_so_cus on sod.partner_id = rep_so_cus.id
	left join account_invoice parent_aci on aci.parent_invoice_id = parent_aci.id
	left join (
		select *
		from account_deposit_account_invoice_rel
		/* Parameter */
		where account_invoice_id in ($P!{IDS})
		) adai on aci.id = adai.account_invoice_id
	left join account_deposit adpt on adpt.id = adai.account_deposit_id
	left join account_invoice parent_inv on aci.parent_invoice_id = parent_inv.id
	left join account_invoice origin_inv on aci.old_inv_id  = origin_inv.id
	left join return_reason rtr on aci.return_reason_id = rtr.id
	inner join res_partner rep_cus on rep_cus.id = aci.partner_id
	left join res_company rec on rec.id = aci.company_id
	left join res_partner rep_com on rep_com.id = rec.partner_id
	left join pos_branch rep_branch on rep_branch.id = aci.branch_id
	left join res_company rec_branch on rec_branch.id = rep_branch.pos_company_id
	INNER JOIN province pv ON rep_branch.province_id = pv.id
	INNER JOIN amphur amp ON rep_branch.amphur_id = amp.id
	INNER JOIN tambon tmb ON rep_branch.tambon_id = tmb.id
	INNER JOIN zip zp ON rep_branch.zip_id = zp.id
	left join res_partner rep_branch2 on rep_branch2.id = rec_branch.partner_id
	left join province prv_com on rep_branch2.province_id = prv_com.id
	INNER JOIN (
		select *
		from account_invoice_line
		/* Parameter */
		where invoice_id in ($P!{IDS})
		and (
			promotion is false
			or (
				promotion is true
				and promotion_name is null
				)
			)
		) ail on aci.id = ail.invoice_id
	LEFT JOIN account_invoice_line_tax alt on ail.id = alt.invoice_line_id
	LEFT JOIN account_tax t on t.id = alt.tax_id
	inner JOIN product_product prp on ail.product_id = prp.id
	INNER JOIN product_template prt on prp.product_tmpl_id = prt.id
	left join (
		select cal_dis_aci.id as aci_id,
		sum(
			case when cal_dis_t.amount is null and cal_dis_ail.free_product_id is null
			then cal_dis_ail.prorate_amount
			when cal_dis_t.amount is null and cal_dis_ail.free_product_id is not null
			then abs(cal_dis_ail.amount_subtotal)
			else 0
			end
			) as discount_non_taxed,
		sum(
			case when cal_dis_t.amount is not null and cal_dis_ail.free_product_id is null
			then cal_dis_ail.prorate_amount
			when cal_dis_t.amount is not null and cal_dis_ail.free_product_id is not null
			then abs(cal_dis_ail.amount_subtotal)
			else 0
			end
			) as discount_taxed
		from (
			select *
			from account_invoice
			/* Parameter */
			where id in ($P!{IDS})
			) cal_dis_aci
		INNER JOIN account_invoice_line cal_dis_ail on cal_dis_aci.id = cal_dis_ail.invoice_id
		LEFT JOIN account_invoice_line_tax cal_dis_alt on cal_dis_ail.id = cal_dis_alt.invoice_line_id
		LEFT JOIN account_tax cal_dis_t on cal_dis_t.id = cal_dis_alt.tax_id
		inner JOIN product_product cal_dis_prp on cal_dis_ail.product_id = cal_dis_prp.id
		inner JOIN product_template cal_dis_prt on cal_dis_prp.product_tmpl_id = cal_dis_prt.id
		group by cal_dis_aci.id
		) cal_dis on aci.id = cal_dis.aci_id
	) flv
group by account_id,
company_name,
company_addr,
doc_no,
date_invoice,
change_cn_detail,
customer_name,
customer_addr,
inv_line_tax,
inv_line_product_id,
inv_line_product_barcode,
inv_line_product_name,
inv_line_unit_price,
amount_total,
amount_tax,
tax,
doc_type,
page,
receipt_no,
round_adjust,
pos_id,
amount_parent_inv,
true_amount,
difference_amount,
discount_line_non_vat,
discount_line_vat,
parent_name,
parent_no,
return_reason,
is_void_order,
is_return_order,
deposit_id,
invoice_state,
parent_invoice_id,
deposit_name
order by
page,
inv_line_product_id]]>
	</queryString>
