	<field name="non_deposit_inv_id" class="java.lang.String"/>
	<field name="is_show_receipt_no" class="java.lang.Boolean"/>
	<field name="is_clear_deposit" class="java.lang.Boolean"/>
	<field name="receipt_no" class="java.lang.String"/>
	<field name="origin_inv_no" class="java.lang.String"/>
