	<queryString>
		<![CDATA[with table_check_have_so_credit_term as (
  select ai.id from
  account_invoice ai
  inner join sale_order so on so.id = ai.so_id
  and so.sale_payment_type = 'credit'
  where ai.id in ($P!{IDS})
  )
select account_id,
company_name,
company_addr,
doc_no,
date_invoice,
(
	case when sale_payment_type = 'credit'
	then 'ใบกำกับภาษี / ใบส่งสินค้า (Tax Invoice / Delivery Order)'
	when sale_payment_type = 'deposit' and not $P{is_print_dpo}
	then 'ใบเสร็จรับเงิน / ใบกำกับภาษี / ใบมัดจำ (Receipt / Tax Invoice / Deposit)'
	--  when pos_id is not null
	--       or sale_payment_type = 'cash'
	--       or invoice_state = 'paid'
	else 'ใบเสร็จรับเงิน / ใบกำกับภาษี (Receipt / Tax Invoice)'
	end
	) as form_name,
change_inv_detail,
customer_name,
customer_addr,
delivery_addr,
contact_name,
contact_phone,
customer_payment_type,
sale_payment_type,
credit_term,
date_due,
inv_line_tax,
inv_line_product_barcode,
inv_line_product_name,
sum(inv_line_quantity) as inv_line_quantity,
inv_line_unit_price,
sum(inv_line_amount) as inv_line_amount,
sum(inv_line_amount_subtotal) as inv_line_amount_subtotal,
amount_total,
amount_tax,
tax,
discount_non_taxed,
discount_taxed,
doc_type,
page,
receipt_no,
round_adjust,
pos_id,
origin_inv_no,
deposit_id,
(
	case when $P{is_print_dpo}
	or (coalesce((select id from table_check_have_so_credit_term), 0) != 0)
	--   pos_id is not null
	--   or (deposit_id is not null
	--       and invoice_state <> 'paid')
	--   or (deposit_id is null
	--       and invoice_state = 'paid')
	then false
	else true
	end
	) as is_show_payment,
(
	case when deposit_id is null
	and pos_id is null
	then true
	else false
	end
	) as is_show_delivery,
(
	case when pos_id is not null
	then true
	else false
	end
	) as is_show_receipt_no,
(
	case when invoice_state = 'paid'
	or pos_id is not null
	or $P{is_print_dpo}
	then true
	else false
	end
	) as is_show_remark,
(
	case when $P{is_print_dpo}
	--   deposit_id is not null
	--   and invoice_state = 'paid'
	--   and pos_id is null
	then true
	else false
	end
	) as is_clear_deposit,
(
	case when deposit_id is null
	and pos_id is null
	then account_id
	else null
	end
	) as non_deposit_inv_id
from (
	select aci.id as account_id,
	(
		rep_com.name  ||
		coalesce(
			case when rep_branch.branch_id = '00000'
			then 'สำนักงานใหญ่'
			else ' สาขา ' || rep_branch.name || coalesce(' สาขาที่ ' || rep_branch.branch_id, '')
			end, ''
			) 
		) as company_name,
	(
		coalesce(rep_branch.street,'') || ' ' ||
		coalesce(rep_branch.moo,'') || ' ' ||
		coalesce(rep_branch.alley,'') || ' ' ||
		coalesce(rep_branch.street2,'') || '<br>' ||
		coalesce(
			case when pv.code = '10'
			then 'แขวง'
			else 'ตำบล'
			end || tmb.name, ''
			) || ' ' ||
		coalesce(
			case when pv.code = '10'
			then 'เขต'
			else 'อำเภอ'
			end || amp.name, ''
			) || ' ' ||
		coalesce(
			case when pv.name like '%กรุงเทพ%'
			then pv.name
			else ('จังหวัด' || pv.name)
			end, ''
			) || ' ' ||
		coalesce(zp.name, '')
		|| ' โทร.' || coalesce(rep_branch.phone,'') ||
		coalesce('<br>เลขประจำตัวผู้เสียภาษีอากร: ' || rep_com.vat, rep_com.number, '')
		) as company_addr,
	aci.number as doc_no,
	(
		TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'dd') || ' ' ||
		case when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '01'
		then 'มกราคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '02'
		then 'กุมภาพันธ์'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '03'
		then 'มีนาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '04'
		then 'เมษายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '05'
		then 'พฤษภาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '06'
		then 'มิถุนายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '07'
		then 'กรกฏาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '08'
		then 'สิงหาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '09'
		then 'กันยายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '10'
		then 'ตุลาคม'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '11'
		then 'พฤศจิกายน'
		when TO_CHAR(DATE(aci.date_invoice + interval '7 hours'), 'mm') = '12'
		then 'ธันวาคม'
		end || ' ' ||
		TO_CHAR(DATE(aci.date_invoice + interval '543 years'), 'yyyy')
		) as date_invoice,
	(
		'*** ยกเลิกใบกำกับภาษีฉบับเดิมเลขที่ ' ||
		origin_inv.number ||
		' *** ' ||
		aci.note
		) as change_inv_detail,
	(
		CASE WHEN rep_cus.parent_id is null
		THEN coalesce(rep_cus.name,'')
		ELSE coalesce(res_par.name,'')
		END
		|| ' ' ||
		coalesce(case when rep_cus.is_company = False
			then ''
			when rep_cus.shop_id = '00000'
			then 'สำนักงานใหญ่'
			else coalesce(' สาขาที่ ' || rep_cus.shop_id, '')
			end,''
			) || ' เลขประจำตัวผู้เสียภาษีอากร ' ||
		coalesce(rep_cus.vat, rep_cus.number, '')
		) as customer_name,
	aci.address_inv as customer_addr,
	(
		CASE WHEN rep_cus.parent_id is null
		THEN coalesce(rep_cus.name,'')
		ELSE coalesce(res_par.name,'')
		END
		|| ' ' ||
		coalesce(cus_delivery.street,'') || ' ' ||
		coalesce(cus_delivery.moo,'') || ' ' ||
		coalesce(cus_delivery.alley,'') || ' ' ||
		coalesce(cus_delivery.street2,'') || ' ' ||
		coalesce(
			case when cus_prv.code = '10'
			then 'แขวง'
			else 'ตำบล'
			end || cus_tmb.name, ''
			) || ' ' ||
		coalesce(
			case when cus_prv.code = '10'
			then 'เขต'
			else 'อำเภอ'
			end || cus_amp.name, ''
			) || ' ' ||
		coalesce(
			case when cus_prv.name like '%กรุงเทพ%'
			then cus_prv.name
			else ('จังหวัด' || cus_prv.name)
			end, ''
			) || ' ' ||
		coalesce(cus_zip.name, '')
		) as delivery_addr,
	con_rpn.name as contact_name,
	(
		coalesce(con_rpn.phone, '') ||
		case when length(coalesce(con_rpn.phone, '')) > 0
		and length(coalesce(con_rpn.mobile, '')) > 0
		then ', ' || con_rpn.mobile
		else coalesce(con_rpn.mobile, '')
		end
		) as contact_phone,
	(
		case when sod.sale_payment_type = 'credit'
		then 'Credit Term'
		when sod.sale_payment_type = 'cash'
		then 'Cash / Credit'
		when sod.sale_payment_type = 'deposit'
		then 'Deposit'
		end
		) as customer_payment_type,
	sod.sale_payment_type as sale_payment_type,
	apt.name as credit_term,
	(
		TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'dd') || ' ' ||
		case when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '01'
		then 'มกราคม'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '02'
		then 'กุมภาพันธ์'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '03'
		then 'มีนาคม'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '04'
		then 'เมษายน'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '05'
		then 'พฤษภาคม'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '06'
		then 'มิถุนายน'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '07'
		then 'กรกฏาคม'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '08'
		then 'สิงหาคม'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '09'
		then 'กันยายน'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '10'
		then 'ตุลาคม'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '11'
		then 'พฤศจิกายน'
		when TO_CHAR(DATE(aci.date_due + interval '7 hours'), 'mm') = '12'
		then 'ธันวาคม'
		end || ' ' ||
		TO_CHAR(DATE(aci.date_due + interval '543 years'), 'yyyy')
		) as date_due,
	case when t.amount is null
	then 'N'
	else 'V'
	end as inv_line_tax,
	prp.id as inv_line_product_id,
	prp.barcode as inv_line_product_barcode,
	prt.name as inv_line_product_name,
	ail.quantity as inv_line_quantity,
	ail.price_unit as inv_line_unit_price,
	ail.amount_subtotal as inv_line_amount,
	ail.amount_subtotal as inv_line_amount_subtotal,
	ail.promotion as inv_line_is_promotion,
	aci.amount_total,
	aci.amount_tax,
	abs(coalesce(t.amount, 0)) as tax,
	abs(coalesce(cal_dis.discount_non_taxed, 0)) as discount_non_taxed,
	abs(coalesce(cal_dis.discount_taxed, 0)) as discount_taxed,
	aci.doc_type,
	aci.page,
	po.inv_no as receipt_no,
	coalesce(po.change_rounding, 0) ,
	(
		case when adai.account_deposit_id is null
		then coalesce(po.change_rounding, 0)
		else coalesce(adai.round_adjust, 0)
		end
		) as round_adjust,
	po.id as pos_id,
	origin_inv.number as origin_inv_no,
	adai.account_deposit_id as deposit_id,
	aci.state as invoice_state
	from (
		select *,
		case when print_time_temp <= 1
		then 'ต้นฉบับ'
		else 'สำเนา'
		end as doc_type,
		1 as page
		from (
			select
			case when $P{is_print_dpo}
			then print_time_dpo
			else print_time
			end as print_time_temp,
			*
			from account_invoice
			/* Parameter */
			where id in ($P!{IDS})
			and state not in ('draft', 'cancel')
			) sub_aci_1
		union all
		select *,
		'สำเนา' as doc_type,
		2 as page
		from (
			select
			case when $P{is_print_dpo}
			then print_time_dpo
			else print_time
			end as print_time_temp,
			*
			from account_invoice
			/* Parameter */
			where id in ($P!{IDS})
			and state not in ('draft', 'cancel')
			) sub_aci_2
		where print_time_temp <= 1
		) aci
	left join pos_order po on aci.pos_id = po.id
	left join sale_order sod on aci.so_id = sod.id
	left join (
		select adai.*, deposit_count.round_adjust
		from account_deposit_account_invoice_rel adai
		left join(
			select
			id as deposit_id,
			max(change_rounding) as round_adjust
			from account_deposit
			where state not in ('draft', 'cancel')
			group by sale_id,id
			order by id
			)deposit_count on deposit_count.deposit_id = adai.account_deposit_id
		/* Parameter */
		where adai.account_invoice_id in ($P!{IDS})
		) adai on aci.id = adai.account_invoice_id
	left join account_invoice origin_inv on aci.old_inv_id  = origin_inv.id
	inner join res_partner rep_cus on rep_cus.id = aci.partner_id
	left join res_partner res_par on res_par.id = rep_cus.parent_id

	inner join res_company rec on rec.id = aci.company_id
	inner join res_partner rep_com on rep_com.id = rec.partner_id
	inner join pos_branch rep_branch on rep_branch.id = aci.branch_id
	inner join res_company rec_branch on rec_branch.id = rep_branch.pos_company_id
	INNER JOIN province pv ON rep_branch.province_id = pv.id
	INNER JOIN amphur amp ON rep_branch.amphur_id = amp.id
	INNER JOIN tambon tmb ON rep_branch.tambon_id = tmb.id
	INNER JOIN zip zp ON rep_branch.zip_id = zp.id
	inner join res_partner rep_branch2 on rep_branch2.id = rec_branch.partner_id
	left join res_partner cus_delivery on aci.partner_shipping_id = cus_delivery.id
	left JOIN province cus_prv ON cus_delivery.province_id = cus_prv.id
	left JOIN amphur cus_amp ON cus_delivery.amphur_id = cus_amp.id
	left JOIN tambon cus_tmb ON cus_delivery.tambon_id = cus_tmb.id
	left JOIN zip cus_zip ON cus_delivery.zip_id = cus_zip.id
	left join res_partner con_rpn on sod.contact_id = con_rpn.id
	INNER JOIN (
		select *
		from account_invoice_line
		/* Parameter */
		where invoice_id in ($P!{IDS})
		and (
			promotion is false
			or (
				promotion is true
				and promotion_name is null
				)
			)
		) ail on aci.id = ail.invoice_id
	LEFT JOIN account_invoice_line_tax alt on ail.id = alt.invoice_line_id
	LEFT JOIN account_tax t on t.id = alt.tax_id
	INNER JOIN product_product prp on ail.product_id = prp.id
	INNER JOIN product_template prt on prp.product_tmpl_id = prt.id
	left join account_payment_term apt on aci.payment_term_id = apt.id
	left join (
		select cal_dis_aci.id as aci_id,
		sum(
			case when cal_dis_t.amount is null and cal_dis_ail.free_product_id is null
			then cal_dis_ail.prorate_amount
			when cal_dis_t.amount is null and cal_dis_ail.free_product_id is not null
			then abs(cal_dis_ail.amount_subtotal)
			else 0
			end
			) as discount_non_taxed,
		sum(
			case when cal_dis_t.amount is not null and cal_dis_ail.free_product_id is null
			then cal_dis_ail.prorate_amount
			when cal_dis_t.amount is not null and cal_dis_ail.free_product_id is not null
			then abs(cal_dis_ail.amount_subtotal)
			else 0
			end
			) as discount_taxed
		from (
			select *
			from account_invoice
			/* Parameter */
			where id in ($P!{IDS})
			) cal_dis_aci
		INNER JOIN account_invoice_line cal_dis_ail on cal_dis_aci.id = cal_dis_ail.invoice_id
		LEFT JOIN account_invoice_line_tax cal_dis_alt on cal_dis_ail.id = cal_dis_alt.invoice_line_id
		LEFT JOIN account_tax cal_dis_t on cal_dis_t.id = cal_dis_alt.tax_id
		inner JOIN product_product cal_dis_prp on cal_dis_ail.product_id = cal_dis_prp.id
		inner JOIN product_template cal_dis_prt on cal_dis_prp.product_tmpl_id = cal_dis_prt.id
		group by cal_dis_aci.id
		) cal_dis on aci.id = cal_dis.aci_id
	) flv
group by account_id,
company_name,
company_addr,
doc_no,
date_invoice,
change_inv_detail,
customer_name,
customer_addr,
delivery_addr,
contact_name,
contact_phone,
customer_payment_type,
sale_payment_type,
credit_term,
date_due,
inv_line_tax,
