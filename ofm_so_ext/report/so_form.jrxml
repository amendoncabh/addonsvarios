	<field name="line_price_total" class="java.math.BigDecimal"/>
	<field name="line_flag_tax" class="java.lang.String"/>
	<field name="line_cal_total" class="java.math.BigDecimal"/>
	<field name="discount_non_taxed" class="java.lang.Double"/>
	<field name="discount_taxed" class="java.lang.Double"/>
	<field name="discount_rate" class="java.lang.Double"/>
	<field name="delivery_fee" class="java.math.BigDecimal"/>
	<variable name="count_line" class="java.lang.Integer" calculation="Count">
		<variableExpression><![CDATA[$V{COLUMN_NUMBER}]]></variableExpression>
	</variable>
	<variable name="sum_product_untaxed" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{line_flag_tax}=='N')?$F{line_cal_total}:0]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
	<variable name="sum_product_taxed" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{line_flag_tax}=='V')?$F{line_cal_total}:0]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
	<variable name="discount" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{discount_non_taxed} + $F{discount_taxed}]]></variableExpression>
	</variable>
	<variable name="total_amount_taxed_discounted" class="java.math.BigDecimal">
		<variableExpression><![CDATA[($V{sum_product_taxed} + $F{delivery_fee}) - $F{discount_taxed}]]></variableExpression>
	</variable>
	<variable name="amount_taxed_discounted" class="java.math.BigDecimal">
		<variableExpression><![CDATA[(($V{total_amount_taxed_discounted} * 100) / (107))]]></variableExpression>
	</variable>
	<variable name="percen_discount" class="java.math.BigDecimal">
		<variableExpression><![CDATA[(($F{discount_rate} / (($V{discount} > 0)? $V{discount}:1)) / $F{line_qty}) * 100]]></variableExpression>
	</variable>
	<variable name="grand_total" class="java.math.BigDecimal">
		<variableExpression><![CDATA[($F{delivery_fee} + $V{sum_product_untaxed} + $V{sum_product_taxed}) - $V{discount}]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
	<variable name="bahttext" class="java.lang.String">
		<variableExpression><![CDATA[org.numbertext.Numbertext.moneytext(new DecimalFormat("#0.00").format($V{grand_total}),"THB","th_TH")]]></variableExpression>
	</variable>
