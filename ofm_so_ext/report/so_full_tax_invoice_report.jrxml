	<queryString>
		<![CDATA[select *,
number as tax_number,
0 AS search_invoice,
'' AS old_invoice,
(now() + INTERVAL '7 hour')::date AS print_date,
(now() + INTERVAL '7 hour')::time AS print_time,
'V' AS status,
round(amount_total,2) AS total,
round(amount_tax,2) AS vat,
round(amount_total,2)-round(amount_tax,2) AS before_vat,
customer_name AS customer_name_var,
customer_address AS customer_address_var ,
customer_address2 AS customer_address2_var,
customer_city AS customer_city_var,
tax_id_customer AS tax_id_customer_var,
customer_province AS customer_province_var,
customer_zip AS customer_zip_var,
shop_id AS shop_id_var,
customer_phone AS customer_phone_var
FROM (
	SELECT
	saa.number,
	0 AS po_id,
	rec.name AS company_name,
	pob.id AS id_branch,
	pob.branch_name,
	pob.sequence AS branch_sequence,
	pob.branch_id,
	rep.vat AS tax_id_company,
	false AS is_return_order,
	pob.street,
	pob.street2,
	pob.city,
	pob.zip,
	COALESCE(pob.phone, '') as phone,
	saa.origin AS inv_no,
	'' AS pos_no,
	DATE(sao.date_order + INTERVAL '7 hours') AS modify_date,
	DATE(sao.date_order + INTERVAL '7 hours') AS date_order,
	pro_com.name AS province,
	rep.vat AS tax_id_customer,
	rep.name AS customer_name,
	rep.street AS customer_address,
	rep.street2 AS customer_address2,
	rep.city AS customer_city,
	rep.zip AS customer_zip,
	pro_cus.name AS customer_province,
	rep.shop_id AS shop_id,
	COALESCE(rep.phone, '') AS customer_phone,
	rep.mobile AS customer_mobile,
	sao.amount_total,
	sao.amount_tax,
	'' AS count_column
	FROM (
		SELECT id,
		amount_tax,
		amount_total,
		branch_id,
		date_order,
		name,
		company_id,
		partner_id
		FROM sale_order
		WHERE DATE(date_order + INTERVAL '7 hours') BETWEEN $P{start_date}::date AND $P{end_date}::date
		) as sao
	INNER JOIN (
		SELECT DISTINCT order_id,
		aci.number,
		aci.origin
		FROM sale_order_line sol
		LEFT JOIN sale_order_line_invoice_rel slr ON sol.id = slr.order_line_id
		LEFT JOIN account_invoice_line ail ON slr.invoice_line_id = ail.id
		LEFT JOIN account_invoice aci ON ail.invoice_id = aci.id
		WHERE aci.state IN ('open', 'paid')
		) saa
	ON sao.id = saa.order_id
	INNER JOIN (
		SELECT *
		FROM pos_branch
		WHERE sequence BETWEEN $P{start_branch}::integer AND $P{end_branch}::integer
		) pob
	ON sao.branch_id = pob.id
	LEFT JOIN res_company pc ON pob.pos_company_id = pc.id
	LEFT JOIN res_partner rpc ON pc.partner_id = rpc.id
	left join province pro_com on rpc.province_id = pro_com.id
	LEFT JOIN res_country_state rcs ON pob.state_id = rcs.id
	LEFT JOIN res_company rec ON sao.company_id = rec.id
	LEFT JOIN res_partner rep ON rep.id = sao.partner_id
	left join province pro_cus on rep.province_id = pro_cus.id
	LEFT JOIN res_country_state rcs2 ON rep.state_id = rcs2.id
	) AS subq
ORDER BY branch_sequence, tax_number, po_id]]>
	</queryString>
