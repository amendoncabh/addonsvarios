		<variableExpression><![CDATA[($F{pos_vat_total} + $F{sale_order_vat}) - ($F{pos_vat_disccount_total}+$F{sale_discount_vat})]]></variableExpression>
	</variable>
	<variable name="sale_no_vat_not_discount_total" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{pos_no_vat_total}+$F{sale_order_no_vat}) - ($F{pos_no_vat_disccount_total}+$F{sale_discount_no_vat})]]></variableExpression>
	</variable>
	<variable name="total_sale" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{pos_vat_total} + $F{pos_no_vat_total} + $F{sale_order_vat} + $F{sale_order_no_vat}]]></variableExpression>
	</variable>
	<variable name="total_discount" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{pos_vat_disccount_total} + $F{pos_no_vat_disccount_total} + $F{sale_discount_vat} + $F{sale_discount_no_vat}]]></variableExpression>
	</variable>
	<variable name="total_net_sale" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$V{sale_vat_not_discount_total} + $V{sale_no_vat_not_discount_total}]]></variableExpression>
	</variable>
	<variable name="total_sale_vat" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{sale_order_vat} + $F{sale_deposit_vat_total}]]></variableExpression>
	</variable>
	<variable name="total_vat_to_gov" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$V{total_sale_vat} - ($F{invoice_vat_clear_deposit} + $F{invoice_vat_return_total} + $F{pos_vat_return_total} + $F{clear_vat_deposit_total})]]></variableExpression>
	</variable>
	<variable name="total_vat_return" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{invoice_vat_clear_deposit} + $F{invoice_vat_return_total} + $F{pos_vat_return_total} + $F{clear_vat_deposit_total}]]></variableExpression>
	</variable>
	<variable name="total_value_product" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$V{total_vat_to_gov} / 1.07]]></variableExpression>
	</variable>
	<variable name="total_product_sale_vat" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$V{total_value_product} * (7 / 100)]]></variableExpression>
	</variable>
	<variable name="total_sale_no_vat" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{sale_order_no_vat} + $F{sale_deposit_no_vat_total}]]></variableExpression>
	</variable>
	<variable name="total_no_vat_return" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{invoice_no_vat_clear_deposit} + $F{invoice_no_vat_return_total} + $F{pos_no_vat_return_total} + $F{clear_no_vat_deposit_total}]]></variableExpression>
	</variable>
	<variable name="total_no_vat_to_gov" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{pos_no_vat_total} + $F{sale_order_no_vat} + $F{sale_deposit_no_vat_total}) - $V{total_no_vat_return}]]></variableExpression>
	</variable>
	<variable name="total_sale_deposit" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$V{total_sale} + $F{sale_deposit_vat_total} + $F{sale_deposit_no_vat_total}]]></variableExpression>
