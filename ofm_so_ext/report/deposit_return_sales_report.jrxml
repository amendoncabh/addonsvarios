	<queryString>
		<![CDATA[with temp_tender as (
			select
				invoice_id,
				sum(all_tender.paid_amount) as paid_total,
				max(payment_date) as payment_date,
				array_to_string(array_agg(type), ', ') as tender
			from
				(
				select
					ail.invoice_id as invoice_id,
					sum(ail.paid_amount) as paid_amount,
					max(ap.payment_date) as payment_date,
					case
						when ajn.type = 'cash' then 'Cash'
						when ajn.type = 'bank' then 'Credit Card'
						else ''
					end as type
				from
					account_invoice_payment_line ail
				inner join account_payment ap on
					ap.id = ail.payment_id
				inner join account_payment_line apl on
					apl.payment_id = ail.payment_id
				inner join account_payment_method_multi apmm on
					apmm.id = apl.payment_method_id
				inner join (
					select
						*
					from
						account_journal ) ajn on
					apmm.journal_id = ajn.id
				group by
					ajn.type,
					ail.invoice_id ) all_tender
			group by
				invoice_id),
			temp_deposit_tender as (
			select
				dep_id,
				sum(all_tender.paid_amount) as paid_total,
				max(payment_date) as payment_date,
				array_to_string(array_agg(type), ', ') as tender
			from
				(
				select
					adl.payment_id as dep_id,
					sum(adl.paid_total) as paid_amount,
					max(dep.date) as payment_date,
					case
						when ajn.type = 'cash' then 'Cash'
						when ajn.type = 'bank' then 'Credit Card'
						else ''
					end as type
				from
					account_deposit_payment_line adl
				inner join account_deposit dep on
					dep.id = adl.payment_id
				inner join (
					select
						*
					from
						account_journal ) ajn on
					adl.journal_id = ajn.id
				group by
					ajn.type,
					adl.payment_id ) all_tender
			group by
				dep_id),
			temp_tax as (
			select
				tcn.invoice_line_id as cnl_id,
				case
					when tax_id is not null then true
					else false
				end as is_tax
			from
				account_invoice_line_tax tcn ),
			temp_deposit as ( select*
			from
			(
			select
				so.id as sale_id,
				dep.name as dep_name,
				so.name as sale_name,
				round(sum(case when inv.type = 'out_invoice' then inv.amount_total_signed when inv.type = 'out_refund' then inv.amount_total_signed*(-1) end), 2) as inv_total,
				round(sum(so.amount_total), 2) as so_total,
				sum(case when inv.type = 'out_invoice' then 0 when inv.type = 'out_refund' then 1 else 0 end) as cn_sum
			from
				account_deposit dep
			inner join sale_order so on
				dep.sale_id = so.id
			inner join account_invoice inv on
				inv.so_id = so.id
			where
				dep.state not in ('draft')
			group by
				so.id,
				dep.name,
				so.name ) dep_data
			where
			inv_total = so_total
			and cn_sum != 0 ) select
				(now() + interval '7 hour')::date as print_date,
				(now() + interval '7 hour')::time as print_time,
				com_logo.company_logo,
				rcn.name || ' ' ||
				case
					when pob.branch_id = '00000' then 'สำนักงานใหญ่'
					else 'สาขา' || pob.name || ' สาขาที่ ' || pob.branch_id
				end as company_name,
				coalesce(pob.street, '') || ' ' || coalesce(pob.moo || ' ', '') || coalesce(pob.alley || ' ', '') || coalesce(pob.street2, '')|| '<br>' || coalesce(case when pv.name like '%กรุงเทพ%' then 'แขวง' else 'ตำบล' end || tmb.name, '') || ' ' || coalesce(case when pv.name like '%กรุงเทพ%' then 'เขต' else 'อำเภอ' end || amp.name, '') || ' ' || coalesce(case when pv.name like '%กรุงเทพ%' then pv.name else ('จังหวัด' || pv.name) end, '') || ' ' || coalesce(zp.name, '') || ' โทร : ' || coalesce(pob.phone, '') || '<br>' || ' เลขประจำตัวผู้เสียภาษีอากร: ' || rpc.vat as address_and_tax,
				so.id,
				cn.date_invoice as date,
				part.name as sale_name,
				dep.id as dep_id,
				case
					when ads.dep_name is not null then ads.dep_name
					else '-'
				end as deposit_no,
				case
					when cn.number is not null then cn.number
					else '-'
				end as cn_no,
				(max(cn.amount_total_signed) + sum(cnl.prorate_amount)) as amount,
				sum(cnl.prorate_amount) as discount,
				(max(cn.amount_total_signed)) as amount_total,
				(max(cn.amount_tax))*(-1) as amount_tax,
				case
					when tax.is_tax is not null
					and tax.is_tax = true then (sum(cnl.price_subtotal))*(-1)
					else 0
				end as amount_untaxed,
				case
					when tax.is_tax is null
					and tax.is_tax = false then (sum(cnl.price_subtotal))*(-1)
					else 0
				end as amount_nontaxed,
				coalesce(dp.paid_total, dpt.paid_total) as paid_total,
				coalesce(dp.tender, dpt.tender) as tender,
				coalesce(dp.payment_date, dpt.payment_date) as payment_date,
				so.branch_id
			from
				account_invoice cn
			inner join account_invoice_line cnl on
				cnl.invoice_id = cn.id
			left join sale_order so on
				so.id = cn.so_id
			inner join account_deposit dep on
				dep.sale_id = so.id
			left join res_users usr on
				usr.id = so.user_id
			left join res_partner part on
				part.id = usr.partner_id
			inner join temp_deposit ads on
				so.id = ads.sale_id
			left join temp_tender dp on
				dp.invoice_id = cn.id
			left join temp_deposit_tender dpt on
				dpt.dep_id = dep.id
			left join temp_tax tax on
				tax.cnl_id = cnl.id
			inner join (
				select
					*
				from
					pos_branch /* Parameter branch_id */
				where
					(id = $P{branch_id}::integer
					or coalesce($P{branch_id}, '') = '') ) pob on
				so.branch_id = pob.id
			inner join (
				select
					*
				from
					res_company /* Parameter company_id */
				where
					id = '3'::integer
					or coalesce('3', '') = '' ) rcn on
				so.company_id = rcn.id
			inner join res_partner rpc on
				rcn.partner_id = rpc.id
			inner join province pv on
				pob.province_id = pv.id
			inner join amphur amp on
				pob.amphur_id = amp.id
			inner join tambon tmb on
				pob.tambon_id = tmb.id
			inner join zip zp on
				pob.zip_id = zp.id
			cross join (
				select
					value || '/web/binary/company_logo?dbname=' || current_database() as company_logo
				from
					ir_config_parameter
				where
					key = 'report.image.url') com_logo
			where
				(sale_payment_type = 'deposit'
				or sale_payment_type is null)
				and (so.branch_id = $P{branch_id}::integer /* Parameter branch_id */
				or coalesce($P{branch_id}, '') = '')
				and (usr.id = $P{user_id}::integer /* Parameter user_id */
				or coalesce($P{user_id}, '') = '')
				and so.state in ('sale',
				'done')
				and cn.state in ('paid')
				and cn.type = 'out_refund'
				and (cn.date_invoice + interval '7 hours')::date >= $P{start_date}::date
				and (cn.date_invoice + interval '7 hours')::date < $P{end_next_date}::date
			group by
				so.id,
				tax.is_tax,
				cn.date_invoice,
				part.name,
				dep.id,
				ads.dep_name,
				dp.invoice_id,
				dp.tender,
				dp.paid_total,
				dp.payment_date,
				dpt.dep_id,
				dpt.tender,
				dpt.paid_total,
				dpt.payment_date,
				cn.number,
				so.branch_id,
				com_logo.company_logo,
				rcn.id,
				rcn.name,
				pob.id,
				zp.id,
				pv.id,
				amp.id,
				tmb.id,
				rpc.vat,
				pob.id,
				pob.street,
				pob.alley,
				pob.street2,
				pob.phone,
				pob.name,
				pob.branch_id,
				pob.moo
			order by
				so.branch_id,
				cn.date_invoice,
				ads.dep_name,
				part.name]]>
	</queryString>
