subtotal as subtotal,
round(amount_total,2) as amount_total ,
round(amount_tax,2) as amount_tax,
round(amount_total,2)-round(amount_tax,2) as before_vat,
round(amount_return,2)  as amount_return,
total_discount*(-1),
printed_receipt,
name_template_id,
vip_discount_product_id,
promotion_discount_product_id,
past_session
from
(
	select
	(SELECT value || '/web/binary/company_logo?dbname=' || current_database()
	FROM ir_config_parameter
	WHERE key = 'report.image.url') as company_logo,
	(
		select count(id)
