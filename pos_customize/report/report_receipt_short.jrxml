	<queryString>
		<![CDATA[select * from
(
select
company_logo,
count_line,
(case when is_return_order=TRUE then coalesce(cash,0.00)*(-1) else coalesce(cash,0.00) end) as cash,
(case when is_return_order=TRUE then coalesce(cash_neg,0.00) else abs(coalesce(cash_neg,0.00)) end) as cash_neg,
id,
pol_id,
pos_no,
company_name,
tax_id,
phone,
branch_name,
date_order,
tax_invoice,
inv_no,
price_unit,
discount_percen,
discount_amount,
name_template,
sale_name,
is_return_order,
vat_name,
(case when is_return_order=TRUE then (round((price_unit*sum(qty) ),2))*(-1) else (round((price_unit*sum(qty) ),2)) end)  as subtotal_line,
(case when is_return_order=TRUE then sum(qty)*(-1) else sum(qty) end) as qty,
 name_template_id,
string_agg(to_char(pol_id,'9G999G999'), ', ') as concat_pol_id,
(case when is_return_order=TRUE then subtotal*(-1) else subtotal end)  as subtotal,
(case when is_return_order=TRUE then round(amount_total,2)*(-1) else round(amount_total,2) end) as amount_total ,
(case when is_return_order=TRUE then round(amount_tax,2)*(-1) else round(amount_tax,2) end) as amount_tax,
(case when is_return_order=TRUE then (round(amount_total,2)-round(amount_tax,2))*(-1) else (round(amount_total,2)-round(amount_tax,2)) end) as before_vat,
(case when is_return_order=TRUE then round(amount_return,2) else abs(round(amount_return,2)) end) as amount_return,
(case when is_return_order=TRUE then total_discount*(-1) else total_discount end) as total_discount,
printed_receipt,
name_template_id,
vip_discount_product_id,
promotion_discount_product_id,
past_session
from
(
	select
	(SELECT value || '/web/binary/company_logo?dbname=' || current_database()
	FROM ir_config_parameter
	WHERE key = 'report.image.url') as company_logo,
	(
		select count(id)
		from 	pos_order_line
		where	order_id=po.id

	) as count_line,
	(
		select sum(absl1.amount)
		from  account_bank_statement_line absl1
		where absl1.pos_statement_id=po.id   and absl1.amount>0
	)as cash,

	(
		select sum(absl2.amount)
		from  account_bank_statement_line absl2
		where absl2.amount<0 and absl2.pos_statement_id=po.id
	)as cash_neg,

	po.id,
	ps.past_session,
	atx.name as vat_name,
	coalesce(pf.pos_no,'XXXXXXXXXXXXX') as pos_no,
	rpc.name as company_name,
	rpc.vat as tax_id,
	coalesce(pb.phone, '02-xxxxxx') as phone,
	pb.branch_name,
	(case when ps.past_session=true then ps.start_at+interval '7 hours'
	    else po.date_order+interval '7 hours' end ) as date_order,
	po.tax_invoice,
	po.inv_no,
	pol.qty as qty,
	pol.price_unit as price_unit,
	pol.discount as discount_percen,
	pol.discount_amount,
	pt.name as name_template,
	pol.id as pol_id,
	pp.id as name_template_id,
	rp.name as sale_name,
	po.is_return_order,
	pf.vip_discount_product_id,
	pf.promotion_discount_product_id,
	(
		select sum(total)
		from(
		  select
		 sum(pol.price_unit*qty) as total
		 from pos_order po1
		 LEFT JOIN pos_session ps ON po1.session_id = ps.id
		 LEFT JOIN pos_config pf ON ps.config_id = pf.id
		 left join pos_order_line pol on po1.id=pol.order_id
		 left JOIN product_product pp ON pol.product_id = pp.id
		 left JOIN product_template pt ON pp.product_tmpl_id = pt.id
		 where po1.id = po.id
		 and pt.id<> pf.vip_discount_product_id
		 and pt.id<> pf.promotion_discount_product_id
		 )as subtotal
	) as subtotal,
	(
		select sum(discount_amount)
		from
		(
		  select pol.discount_amount*pol.qty as discount_amount
		  from pos_order po1
		  left join pos_order_line pol on po1.id=pol.order_id
		  where po1.id = po.id
		  )as sub_q
	)as total_discount,
	po.amount_total,
	po.amount_tax,
	po.amount_return,
	po.printed_receipt
	from pos_order po
	LEFT JOIN pos_session ps ON po.session_id = ps.id
	LEFT JOIN pos_config pf ON ps.config_id = pf.id
	LEFT JOIN pos_branch pb ON pf.branch_id = pb.id
	LEFT JOIN res_company pc ON pb.pos_company_id = pc.id
	LEFT JOIN res_partner rpc ON pc.partner_id = rpc.id
	left join pos_order_line pol on po.id=pol.order_id
	left join product_product pp on pol.product_id=pp.id
	left join product_template pt on pp.product_tmpl_id=pt.id
	left JOIN product_category pcg ON pt.categ_id = pcg.id
	left join res_users ru on ru.id=po.user_id
	left join res_partner rp on rp.id=ru.partner_id
	left join account_tax_pos_order_line_rel atp on atp.pos_order_line_id = pol.id
	left join account_tax atx on atp.account_tax_id=atx.id
	WHERE date(po.date_order + interval '7 hours') between $P{start_date}::date and $P{end_date}::date
	and po.pos_reference is not null
	and pf.branch_id = $P{branch_id}::int
	and po.amount_total>=0
	order by pol.id
) as pp
group by
past_session,
company_logo,
count_line,
cash,
cash_neg,
id,
pos_no,
company_name,
tax_id,
phone,
branch_name,
date_order,
tax_invoice,
inv_no,
price_unit,
discount_percen,
discount_amount,
name_template,
vat_name,
sale_name,
is_return_order,
before_vat,
name_template_id,
subtotal,
amount_total,
amount_tax,
pol_id,
amount_return,
total_discount,
printed_receipt,
name_template_id,
vip_discount_product_id,
promotion_discount_product_id
UNION

select
company_logo,
count_line,
