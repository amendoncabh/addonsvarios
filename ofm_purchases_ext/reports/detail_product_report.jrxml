	<parameter name="checkzero" class="java.lang.String"/>
	<queryString>
		<![CDATA[WITH tb_purchase as (
    select puo_line.product_id, 
        sum(puo_line.product_qty) as product_qty,
        sum(puo_line.product_qty_available) as qty_invoiced
    from (
        select *
        from purchase_order
        where state = 'completed'
        and (date_part('year', date_order) <  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER
    or (date_part('month', date_order) <=  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'MM'))::INTEGER
    and date_part('year', date_order) =  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER ))
        and branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = ''
        and company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = ''
    )puo
    inner join ofm_purchase_order_line puo_line on puo_line.order_id = puo.id
    group by puo_line.product_id
    order by product_id
),
tb_so_deposit_no_inv as(
    select so.id
    from (
        select *
        from sale_order 
        where state = 'sale'
        and (date_part('year', so_date_order) <  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER
        or (date_part('month', so_date_order) <=  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'MM'))::INTEGER
        and date_part('year', so_date_order) =  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER )  )
        and branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '' 
        and company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = ''
    )so
    left join (
        select *
        from account_invoice 
        where so_id is not null
    )inv on inv.so_id = so.id
    where inv.id is null  
),
tb_so_deposit_order as(
    select so_l.product_id, 
        sum(so_l.product_uom_qty) as product_qty
    from tb_so_deposit_no_inv so
    inner join sale_order_line so_l on so_l.order_id = so.id
    group by so_l.product_id
    order by product_id
),
tb_sale_order_month as (
    select 
        to_char(inv.date_invoice, 'YYYY-MM') as date_invoice,
        inv_l.product_id,
        sum(inv_l.quantity) as quantity 
    from (
        select id, date_invoice
        from account_invoice 
        where so_id is not null
        and state in ('open', 'paid')
        and type = 'out_invoice' 
    and (date_part('year', date_invoice) <  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER
    or (date_part('month', date_invoice) <=  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'MM'))::INTEGER
    and date_part('year', date_invoice) =  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER ) )
        and branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '' 
        and company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = ''
    )inv
    inner join (
        select * 
        from account_invoice_line 
        where promotion_name is null
    )inv_l on inv_l.invoice_id = inv.id
    group by to_char(inv.date_invoice, 'YYYY-MM'),
        inv_l.product_id
    order by to_char(inv.date_invoice, 'YYYY-MM')
)
select distinct on (avp.id, branch_id, pdt.categ_id)

                date_purchase.vender_code,
                date_purchase.vender_name,
                (
                case when pdept.dept_parent_id is not null
                         then parent_pdept.ofm_dept_id
                         else pdept.ofm_dept_id
                end
             ) as dept_id,
             (
                case when pdept.dept_parent_id is not null
                         then pdept.ofm_sub_dept_id
                end
             ) as sub_dept_id,
    pdept.ofm_sub_class_id as sub_class,

    pdc.cat_id as cat_id,
    pdc.sub_cat_id as sub_cat_id,
    pdc.name as cat_name,
    pdb.brand_th as brand_name,

    pd.id,
    pdt.default_code,
    pdt.sku_ofm as sku,
    pd.barcode as barcode,
    pdt.name product_name,
    pdu_sale.name as product_uom_sale_name,
    pdu_po.name as product_uom_po_name,

    coalesce(pdt.price_normal,0) as price_in_vat,
    (coalesce(pdt.price_normal,0) - ((coalesce(pdt.price_normal,0) * coalesce(atx.Amount,0))/100) ) as price_non_vat,
    coalesce(pdt.price_promotion,0) as price__promotion_in_vat,
    (coalesce(pdt.price_promotion,0) - ((coalesce(pdt.price_promotion,0) * coalesce(atx.Amount,0))/100) ) as price__promotion_non_vat,
    (pdt.date_promotion_start + interval '7 hour') as date_promotion_start,
    (pdt.date_promotion_end + interval '7 hour') as date_promotion_end,
    coalesce(p_sub.price,0) as price_purcas_non_vat,
    (coalesce(p_sub.price,0) + ((coalesce(p_sub.price,0) * coalesce(sub_atx.Amount,0))/100) ) as price_purcas_in_vat,
    coalesce(avp.remain_product_qty, 0)as remain_product_qty,
    coalesce(tb_so_o.product_qty, 0) as so_order,
    case when coalesce(tb_so_o.product_qty, 0) = 0
    then 0
    else coalesce(avp.remain_product_qty, 0) - coalesce(tb_so_o.product_qty, 0) 
    end as total_sale,
    
    coalesce(tb_pur_o.qty_invoiced, 0) as pur_order,
    coalesce(stk_who.product_min_qty, 0) as min,
    coalesce(stk_who.product_max_qty, 0) as max,

    to_char(date_trunc('month', ($P{year}||'-'||$P{month}||'-01')::date) - interval '2 month' , 'YYYY-MM-dd') as tb_date_m3,
    coalesce(tb_sale_month3.quantity, 0) as tb_m3,

    to_char(date_trunc('month', ($P{year}||'-'||$P{month}||'-01')::date) - interval '1 month' , 'YYYY-MM-dd') as tb_date_m2,
    coalesce(tb_sale_month2.quantity, 0) as tb_m2,

    to_char(date_trunc('month', ($P{year}||'-'||$P{month}||'-01')::date) - interval '0 month' , 'YYYY-MM-dd') as tb_date_m1,
    coalesce(tb_sale_month1.quantity, 0) as tb_m1,
    
    (coalesce(tb_sale_month1.quantity, 0) + coalesce(tb_sale_month2.quantity, 0) + coalesce(tb_sale_month3.quantity, 0) ) / 3 as Average,

    coalesce(tb_sale_to_month.quantity, 0) as tb_m_today,
    
    case when ((coalesce(tb_sale_month1.quantity, 0) + coalesce(tb_sale_month2.quantity, 0) + coalesce(tb_sale_month3.quantity, 0) )/3) = 0
        then 0
        else coalesce(tb_so_o.product_qty, 0) / ((coalesce(tb_sale_month1.quantity, 0) + coalesce(tb_sale_month2.quantity, 0) + coalesce(tb_sale_month3.quantity, 0) )/3)
    end as to_sale,

    pdt.prod_status as product_status,
    pdt.delivery_fee_ofm as delivery_fee,
    pdt.prod_remark_ofm as note,
    'isTop80' as isTop
    ,pdt.company_id 
    ,avp.branch_id 
             
from (
            select distinct on (product_id, branch_id)
             id,
             pos_id,
             po_id,
             move_date,
             branch_id,
             product_id,
             product_uom_id,
             remain_product_qty,
             remain_product_amount,
             product_average_price
            from average_price avp
            where (date_part('year', move_date) <  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER
                         or (date_part('month', move_date) <=  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'MM'))::INTEGER
                                 and date_part('year', move_date) =  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER ))
            and branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = ''
            order by product_id, branch_id, id DESC, move_date DESC
         ) avp
inner join product_product pd on avp.product_id = pd.id
inner join (
                        select *
                        from product_template
            where (company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = '' )
    and ($P{product_status} like '%,' || prod_status || ',%' or COALESCE($P{product_status}, '') = '')
    and (($P!{product_ids}) IS NULL OR id IN ($P!{product_ids}) )
    
         ) pdt on pd.product_tmpl_id = pdt.id
left JOIN product_brand pdb on pdb.id = pdt.brand_id

left join (
    select product_tmpl_id,
        company_id,
        branch_id,
        price
    from product_supplierinfo 
)p_sub on p_sub.product_tmpl_id = pdt.id 
        and p_sub.branch_id = avp.branch_id and p_sub.company_id = pdt.company_id  

left join product_supplier_taxes_rel pstr on pstr.prod_id = pdt.id
left join account_tax vender_acc_t on vender_acc_t.id = pstr.tax_id
inner join product_uom pdu on pdu.id = avp.product_uom_id

inner join product_uom pdu_sale on pdu_sale.id = pdt.uom_id
inner join product_uom pdu_po on pdu_po.id = pdt.uom_po_id

LEFT JOIN product_taxes_rel p_tax_l on p_tax_l.prod_id = pdt.id
INNER JOIN account_tax atx on atx.id = p_tax_l.tax_id and atx.company_id = pdt.company_id

LEFT JOIN product_supplier_taxes_rel sub_tax_l on sub_tax_l.prod_id = pdt.id
INNER JOIN account_tax sub_atx on sub_atx.id = sub_tax_l.tax_id and sub_atx.company_id = pdt.company_id

inner join (
                        select *
                        from product_category
        where id in ($P!{product_cate_ids}) or ($P!{product_cate_ids}) is null
         ) pdc on pdt.categ_id = pdc.id
left join (
         select distinct on (s_avp.product_id, s_avp.branch_id)
            s_avp.product_id,
            s_avp.branch_id,
            move_date purchase_date,
            rpn.name vender_name,
            rpn.ref vender_code
         from average_price s_avp
         inner join purchase_order po on po.id = s_avp.po_id
         inner join res_partner rpn on rpn.id = po.partner_id
         where po_id is not null
         and (date_part('year', move_date) <  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER
    or (date_part('month', move_date) <=  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'MM'))::INTEGER
    and date_part('year', move_date) =  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER ))           
         order by s_avp.product_id,
                s_avp.branch_id,
                s_avp.id DESC,
                s_avp.move_date DESC
) date_purchase on date_purchase.product_id = avp.product_id
                                             and avp.branch_id = date_purchase.branch_id
left join (
                     select distinct on (product_id, branch_id)
            product_id,
            branch_id,
            move_date sales_date
         from average_price
         where pos_id is not null
    and (date_part('year', move_date) <  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER
    or (date_part('month', move_date) <=  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'MM'))::INTEGER
    and date_part('year', move_date) =  (to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY'))::INTEGER ))
         order by product_id,
                            branch_id,
                            id DESC,
                            move_date DESC
) date_sales on date_sales.product_id = avp.product_id
                                                    and avp.branch_id = date_sales.branch_id
left join(
    select product_id, branch_id, company_id, product_min_qty, product_max_qty
    from stock_warehouse_orderpoint 
)stk_who on  stk_who.product_id = pd.id and stk_who.company_id = avp.branch_id and stk_who.branch_id =  pdt.company_id 

left join tb_purchase tb_pur_o on tb_pur_o.product_id = pd.id
left join tb_so_deposit_order tb_so_o on tb_so_o.product_id = pd.id
left join(
    select *
    from tb_sale_order_month
    where date_invoice = to_char(date_trunc('month', (now())::date) - interval '1 month' , 'YYYY-MM')
)tb_sale_to_month on tb_sale_to_month.product_id = pd.id

left join(
    select *
    from tb_sale_order_month
    where date_invoice = to_char(date_trunc('month', ($P{year}||'-'||$P{month}||'-01')::date) - interval '0 month' , 'YYYY-MM') 
)tb_sale_month1 on tb_sale_month1.product_id = pd.id

left join(
    select *
    from tb_sale_order_month
    where date_invoice = to_char(date_trunc('month', ($P{year}||'-'||$P{month}||'-01')::date) - interval '1 month' , 'YYYY-MM')
)tb_sale_month2 on tb_sale_month2.product_id = pd.id

left join(
    select *
    from tb_sale_order_month
    where date_invoice = to_char(date_trunc('month', ($P{year}||'-'||$P{month}||'-01')::date) - interval '2 month' , 'YYYY-MM') 
)tb_sale_month3 on tb_sale_month3.product_id = pd.id

left join (
                        select *
                        from pos_branch pob
            where pob.id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = ''
         ) pob on pob.id = avp.branch_id
inner join (
                        select *
                        from res_company rcp
            where rcp.id = $P{company_id}::integer or COALESCE($P{company_id}, '') = ''
         ) rcp on rcp.id = pob.pos_company_id
inner join province pv on pob.province_id = pv.id
inner join amphur amp on pob.amphur_id = amp.id
inner join tambon tmb on pob.tambon_id = tmb.id
inner join zip zp ON pob.zip_id = zp.id
inner join res_partner rpn on rpn.id = rcp.partner_id
left join ofm_product_dept pdept on pdept.id = pdt.dept_ofm
left join ofm_product_dept parent_pdept on parent_pdept.id = pdept.dept_parent_id
    where (avp.remain_product_qty <= 0) or COALESCE($P{checkzero}::boolean, FALSE) = FALSE 
order by branch_id,
                 pdt.categ_id,
                 avp.id]]>
	</queryString>
