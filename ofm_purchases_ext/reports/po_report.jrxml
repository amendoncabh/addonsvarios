		<parameter name="po_status" class="java.lang.String"/>
		<parameter name="product_status" class="java.lang.String"/>
		<parameter name="type_to_ofm" class="java.lang.String"/>
		<parameter name="stock_received" class="java.lang.String"/>
		<queryString>
			<![CDATA[select aiv.reference as col_invoice_no,
       pco.name as po_doc_no,
       case when pco.state = 'draft'
            then 'PR'
            when pco.state = 'sent'
            then 'PR Sent'
            when pco.state = 'pending'
            then 'Pending'
            when pco.state = 'completed'
            then 'Completed'
            when pco.state = 'to approve'
            then 'To Approve'
            when pco.state = 'purchase'
            then 'Purchase Order'
            when pco.state = 'done'
            then 'Locked'
       end as po_status,
       vendor_rpn.ref as vendor_code,
       vendor_rpn.name as vendor_name,
       CASE WHEN sdept.dept_parent_id ISNULL
            THEN sdept.ofm_dept_id
	    ELSE dept.ofm_dept_id
       END AS dept_id,
       CASE WHEN sdept.dept_parent_id ISNULL
            THEN ''
	    ELSE sdept.ofm_sub_dept_id
       END AS sub_dept_id,
       pcg.cat_id as cat_id,
       pcg.sub_cat_id as sub_cat_id,
       pcg.name as product_category,
       pdb.brand_th as product_brand,
       prp.default_code as product_code,
       pdt.sku_ofm as sku,
       pdt.name as product_name,
       pdu.name as product_uom,
       pcl.qty_invoiced as qty_invoiced,
       pcl.price_unit as price_untaxed,
       pcl.price_subtotal as price_subtotal,
       pdt.prod_status as product_status,
       case when pco.type_to_ofm = 'fulfillment'
            then 'Fulfilment to Franchise'
            when pco.type_to_ofm = 'store'
            then 'Send to Store'
            when pco.type_to_ofm = 'customer'
            then 'Send to Customer'
       end as delivery_destination
from (
      select *
      from purchase_order
      where state <> 'cancel'
            /* Parameter */
            and (company_id = $P{company_id}::integer)
            and (branch_id = $P{branch_id}::integer)
            and (name like '%' || $P{po_no} || '%' or COALESCE($P{po_no}, '') = '')
            and (DATE(date_order + interval '7 hour') between $P{start_date}::DATE and $P{end_date}::DATE)
            and ($P{po_status} like '%,' || state || ',%' or COALESCE($P{po_status}, '') = '')
            and ($P{type_to_ofm} like '%,' || type_to_ofm || ',%' or COALESCE($P{type_to_ofm}, '') = '')
     ) pco
inner join purchase_order_line pcl on pco.id = pcl.order_id
inner join (
	    select s_spk.*
            from stock_picking s_spk
            inner join (
		        select id
		        from stock_picking_type
		        where code = 'incoming'
		       ) spt on s_spk.picking_type_id = spt.id
	       where (s_spk.state = $P{stock_received} or COALESCE($P{stock_received}, '') = '')
           ) spk on pco.group_id = spk.group_id
left join (
	   select *
	   from account_invoice
	   where state not in ('draft', 'cancel')
	         and type = 'in_invoice'
	  ) aiv on spk.id = aiv.picking_id
left join res_partner vendor_rpn on pco.partner_id = vendor_rpn.id
inner join product_product prp on pcl.product_id = prp.id
inner join (
            select *
            from product_template
            /* Parameter */
            where ($P{product_status} like '%,' || prod_status || ',%' or COALESCE($P{product_status}, '') = '')
           ) pdt on prp.product_tmpl_id = pdt.id
LEFT JOIN ofm_product_dept sdept ON pdt.dept_ofm = sdept.id
LEFT JOIN ofm_product_dept dept ON sdept.dept_parent_id = dept.id
inner join product_uom pdu on pcl.product_uom = pdu.id
inner join product_category pcg on pdt.categ_id = pcg.id
inner join product_brand pdb on pdt.brand_id = pdb.id
order by pco.name, pcl.id]]>
		</queryString>
