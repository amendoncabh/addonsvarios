	<subDataset name="journal">
		<parameter name="pos_id" class="java.lang.Integer"/>
		<parameter name="deposit_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="non_deposit_inv_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select (
        case when is_credit_card is true
             then card_type || ': ' || overlay(credit_card_no placing 'xxxxxx' from 7 for 6)
             when amount < 0 then 'Change CASH'
             else journal_name
        end
       ) as journal_name,
       abs(sum(amount)) as amount
from (
      select aj.name as journal_name,
             absl.amount as amount,
             absl.credit_card_type as card_type,
             coalesce(absl.credit_card_no,'') as credit_card_no,
             aj.is_credit_card as is_credit_card
      from pos_order pod
      inner join account_bank_statement_line absl on absl.pos_statement_id = pod.id
      inner join account_journal aj on absl.journal_id = aj.id
      /* Parameter */
      where pod.id = $P{pos_id}
      group by aj.id,
               absl.amount,
               pod.is_return_order,
               absl.credit_card_no,
               absl.credit_card_type
     ) pp
group by journal_name,
	 is_credit_card,
	 credit_card_no,
	 card_type,
	 amount
union all
select (
        case when aj.is_credit_card is true
             then adp.tender || ': '
                  || overlay(adp.credit_card_no placing 'xxxxxx' from 7 for 6)
             else aj.name
        end
       ) as journal_name,
       sum(adp.paid_total) as amount
from account_deposit_payment_line adp
inner join account_journal aj on adp.journal_id = aj.id
/* Parameter */
where adp.payment_id = $P{deposit_id}
group by aj.name,
     aj.is_credit_card,
     adp.tender,
     adp.credit_card_no
union all
select (
        case when apm.name = 'Credit Card'
             then apm.name || ': '
                  || coalesce(overlay(apl.cheque_number placing 'xxxxxx' from 7 for 6), '')
             else apm.name
        end
       ) as journal_name,
       apl.paid_total as amount
from account_invoice_payment_line aip
inner join account_payment_line apl on aip.payment_id = apl.payment_id
inner join account_payment_method_multi apm on apl.payment_method_id = apm.id
/* Parameter */
where aip.invoice_id = $P{non_deposit_inv_id}]]>
		</queryString>
		<field name="journal_name" class="java.lang.String"/>
		<field name="amount" class="java.lang.Double">
			<fieldDescription><![CDATA[Amount]]></fieldDescription>
		</field>
