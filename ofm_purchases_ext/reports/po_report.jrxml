	<subDataset name="ds_po_summary">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="localhost_OFM_SALE_test"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="company_id" class="java.lang.String"/>
		<parameter name="branch_id" class="java.lang.String"/>
		<parameter name="po_no" class="java.lang.String"/>
		<parameter name="start_date" class="java.lang.String"/>
		<parameter name="end_date" class="java.lang.String"/>
		<parameter name="po_status" class="java.lang.String"/>
		<parameter name="product_status" class="java.lang.String"/>
		<parameter name="type_to_ofm" class="java.lang.String"/>
		<parameter name="stock_received" class="java.lang.String"/>
		<queryString>
			<![CDATA[select pco.name as po_no,
       DATE(pco.date_order + interval '7 hours') as po_date,
       case when pco.state = 'draft'
            then 'PR'
            when pco.state = 'sent'
            then 'PR Sent'
            when pco.state = 'pending'
            then 'Pending'
            when pco.state = 'completed'
            then 'Completed'
            when pco.state = 'to approve'
            then 'To Approve'
            when pco.state = 'purchase'
            then 'Purchase Order'
            when pco.state = 'done'
            then 'Locked'
       end as po_status,
       vendor_rpn.ref as vendor_code,
       vendor_rpn.name as vendor_name,
       apt.name as payment_term,
       sum(pcl.price_subtotal) as amount_untaxed,
       DATE(pco.date_planned + interval '7 hours') as date_planned,
       case when spk.state = 'done'
	    then DATE(spk.date_done + interval '7 hours') 
	    else null::date
       end as date_done,
       case when spk.state = 'done'
	    then 'Received'
	    when spk.state = 'draft'
	    then 'Draft'
	    when spk.state = 'cancel'
	    then 'Cancelled'
	    when spk.state = 'reject'
	    then 'Rejected'
	    else 'Waiting Receive'
       end as picking_status,
       requester_rpn.name as requester_name,
       pco.comment_po
from (
      select *
      from purchase_order
      where state <> 'cancel'
            /* Parameter */
            and (company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = '')
            and (branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '')
            and (name like '%' || $P{po_no} || '%' or COALESCE($P{po_no}, '') = '')
            and (DATE(date_order + interval '7 hour') between $P{start_date}::DATE and $P{end_date}::DATE)
            and ($P{po_status}  like '%,' || state || ',%')
            and ($P{type_to_ofm} like '%,' || type_to_ofm || ',%')
     ) pco
inner join purchase_order_line pcl on pco.id = pcl.order_id
inner join (
	   select s_spk.*
           from stock_picking s_spk
           inner join (
		       select id
		       from stock_picking_type
		       where code = 'incoming'
		      ) spt on s_spk.picking_type_id = spt.id
	       where (s_spk.state = $P{stock_received}::varchar or COALESCE($P{stock_received}, '') = '')
          ) spk on pco.group_id = spk.group_id
inner join product_product prp on pcl.product_id = prp.id
inner join (
            select *
            from product_template
            where ($P{product_status} like '%,' || prod_status || ',%' or COALESCE($P{product_status}, '') = '')
           ) pdt on prp.product_tmpl_id = pdt.id
inner join res_partner vendor_rpn on pco.partner_id = vendor_rpn.id
left join account_payment_term apt on pco.payment_term_id = apt.id
inner join res_users requester_rus on pco.create_uid = requester_rus.id
inner join res_partner requester_rpn on requester_rus.partner_id = requester_rpn.id
group by pco.name,
         pco.date_order,
         pco.state,
         vendor_rpn.id,
         apt.id,
         pco.date_planned,
         spk.date_done,
         spk.state,
         requester_rpn.id,
         pco.comment_po
order by pco.name]]>
		</queryString>
