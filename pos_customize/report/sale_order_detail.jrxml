	<parameter name="start_date" class="java.lang.String"/>
	<parameter name="end_date" class="java.lang.String"/>
	<parameter name="branch_id" class="java.lang.String"/>
	<parameter name="start_category" class="java.lang.String"/>
	<parameter name="end_category" class="java.lang.String"/>
	<parameter name="start_branch" class="java.lang.String"/>
	<parameter name="end_branch" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
to_char(pcg.sequence, '99909') as id_cate,
pcg.id as categories_id,
pcg.name as category,
rpc.name as company_name,
pb.branch_name,
pb.branch_id,
pb.id as pb_id,
rpc.street,
rpc.street2,
rpc.city,
rpc.zip,
pcg.sequence,
rpc.phone,
rcs.name as province ,
(now()+interval '7 hour')::date as print_date,
(now()+interval '7 hour')::time as print_time,
(
	select branch_name
	from
	(
		select pb1.branch_name as branch_name
		from  pos_branch pb1
		where (case when $P{start_branch}::integer<$P{end_branch}::integer then( pb1.id = $P{start_branch}::integer)
			when $P{start_branch}::integer>$P{end_branch}::integer then( pb1.id = $P{end_branch}::integer)
			when $P{start_branch}::integer=$P{end_branch}::integer then( pb1.id = $P{end_branch}::integer)
			end )
		) as temp
	) as start_branch_name,
(
	select branch_name
	from
	(
		select pb1.branch_name as branch_name
		from  pos_branch pb1
		where (case when $P{end_branch}::integer>$P{start_branch}::integer then(pb1.id = $P{end_branch}::integer)
			when $P{end_branch}::integer<$P{start_branch}::integer then(pb1.id = $P{start_branch}::integer)
			when $P{start_branch}::integer=$P{end_branch}::integer then( pb1.id = $P{end_branch}::integer)

			end )
		) as temp
	) as end_branch_name,
(
	select branch_id
	from
	(
		select pb1.branch_id as branch_id
		from  pos_branch pb1
		where (case when $P{start_branch}::integer< $P{end_branch}::integer then(pb1.id = $P{start_branch}::integer)
			when $P{start_branch}::integer> $P{end_branch}::integer then(pb1.id = $P{end_branch}::integer)
			when $P{start_branch}::integer=$P{end_branch}::integer then( pb1.id = $P{end_branch}::integer)


			end )
		) as temp
	) as start_branch_id,
(
	select branch_id
	from
	(
		select pb1.branch_id as branch_id
		from  pos_branch pb1
		where (case when $P{end_branch}::integer >$P{start_branch}::integer then(pb1.id = $P{end_branch}::integer)
			when $P{end_branch}::integer <$P{start_branch}::integer then(pb1.id = $P{start_branch}::integer)
			when $P{start_branch}::integer=$P{end_branch}::integer then( pb1.id = $P{end_branch}::integer)
			end )
		) as temp
	) as end_branch_id,
(
	select category_name
	from
	(
		select pcg1.name as category_name
		from  product_category  pcg1
		where (case when $P{start_category}::integer<$P{end_category}::integer then( pcg1.id = $P{start_category}::integer)
			when $P{start_category}::integer>$P{end_category}::integer then( pcg1.id = $P{end_category}::integer)
			when $P{start_category}::integer=$P{end_category}::integer then( pcg1.id = $P{end_category}::integer)
			end )
		) as temp
	) as start_category_name,
(
	select category_name
	from
	(
		select pcg1.name as category_name
		from  product_category  pcg1
		where (case when $P{end_category}::integer>$P{start_category}::integer then(pcg1.id = $P{end_category}::integer)
			when $P{end_category}::integer<$P{start_category}::integer then(pcg1.id = $P{start_category}::integer)
			when $P{start_category}::integer=$P{end_category}::integer then( pcg1.id = $P{end_category}::integer)

			end )
		) as temp
	) as end_category_name,
(
	select category_id
	from
	(
		select to_char(pcg1.sequence, '99909') as category_id
		from  product_category  pcg1
		where (case when $P{start_category}::integer< $P{end_category}::integer then(pcg1.id= $P{start_category}::integer)
			when $P{start_category}::integer> $P{end_category}::integer then(pcg1.id = $P{end_category}::integer)
			when $P{start_category}::integer=$P{end_category}::integer then( pcg1.id = $P{end_category}::integer)


			end )
		) as temp
	) as start_category_id,
(
	select category_id
	from
	(
		select to_char(pcg1.sequence, '99909') as category_id
		from  product_category  pcg1
		where (case when $P{end_category}::integer >$P{start_category}::integer then(pcg1.id = $P{end_category}::integer)
			when $P{end_category}::integer <$P{start_category}::integer then(pcg1.id = $P{start_category}::integer)
			when $P{start_category}::integer=$P{end_category}::integer then(pcg1.id = $P{end_category}::integer)
			end )
		) as temp
	) as end_category_id
from pos_order po
LEFT JOIN pos_session ps ON po.session_id = ps.id
LEFT JOIN pos_config pf ON ps.config_id = pf.id
LEFT JOIN pos_branch pb ON pf.branch_id = pb.id
LEFT JOIN res_company pc ON pb.pos_company_id = pc.id
LEFT JOIN res_partner rpc ON pc.partner_id = rpc.id
left join pos_order_line pol on po.id=pol.order_id
left join res_users ru on ru.id=po.company_id
left join res_country_state rcs on rpc.state_id=rcs.id
left join res_partner rp on rp.id=rpc.id
left JOIN product_product pp ON pol.product_id = pp.id
left JOIN product_template pt ON pp.product_tmpl_id = pt.id
left JOIN product_category pcg ON pt.categ_id = pcg.id
where DATE(po.date_order+interval '7 hours') BETWEEN
$P{start_date}::date AND  $P{end_date}::date
and (case when $P{start_branch}::integer>$P{end_branch}::integer then(pb.id BETWEEN $P{end_branch} ::integer and $P{start_branch}::integer)
	when $P{start_branch}::integer=$P{end_branch}::integer then( pb.id = $P{start_branch}::integer)
	when  $P{start_branch}::integer<$P{end_branch}::integer  then(pb.id BETWEEN $P{start_branch}::integer and $P{end_branch} ::integer)
	end)
and (case when $P{start_category}::integer>$P{end_category}::integer then(pcg.id BETWEEN $P{end_category} ::integer and $P{start_category}::integer)
	when $P{start_category}::integer=$P{end_category}::integer then( pcg.id = $P{start_category}::integer)
	when  $P{start_category}::integer<$P{end_category}::integer  then(pcg.id BETWEEN $P{start_category}::integer and $P{end_category} ::integer)
	end)
group by pcg.id,
pcg.name,
pb.branch_name,
pb.branch_id,
pcg.sequence,
pcg.sequence,
rcs.name ,
rpc.id,
pb.id
order by pb.branch_id,pcg.sequence]]>
	</queryString>
