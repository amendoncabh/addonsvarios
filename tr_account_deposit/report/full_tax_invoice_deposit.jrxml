	<queryString>
		<![CDATA[WITH table_customer_addr as (
	select
	rp.id as rp_id,
	concat(
		case when street is not null then ' ' || street else '' end,
		case when moo is not null then ' ' || moo else '' end,
		case when alley is not null then ' ' || alley else '' end,
		case when street2 is not null then ' ' || street2 else '' end,
		case when tm is not null then
		case
		when pro.name_eng = 'Bangkok'
		then ' แขวง'
		else ' ตำบล'
		end || tm.name else '' end,
		case when amp is not null then
		case
		when pro.name_eng = 'Bangkok'
		then ' เขต'
		else ' อำเภอ'
		end || amp.name else '' end,
		case when pro is not null then
		case
		when pro.name_eng = 'Bangkok'
		then ' '
		else ' จังหวัด'
		end || pro.name else '' end,
		case when zip is not null then ' ' || zip else '' end
		) as customer_addr
	from res_partner rp
	left join tambon tm on tm.id = rp.tambon_id
	left join amphur amp on amp.id = rp.amphur_id
	left join province pro on pro.id = rp.province_id
	),
deposit_customer_id as (
	select
	partner_id
	from sale_order
	where id = ($P!{IDS}) and state not in ('draft')
	limit 1
	),
deposit_count as (
	select
	id as deposit_id,
	name as deposit_name,
	sale_id,
	print_time_dpo as print_time,
	max(change_rounding) as round_adjust,
	date as date_deposit
	from account_deposit
	where sale_id = ($P!{IDS}) and state not in ('draft', 'cancel')
	group by sale_id,id
	limit 1
	),
ir_property_temp as (
	select distinct
	split_part(ir_p.res_id, ',', 2)::int as res_id,
	split_part(ir_p.value_reference, ',', 2)::int as value_reference,
	apt_res.name as paymane_term_id
	from ir_property ir_p
	left join account_payment_term apt_res on apt_res.id = split_part(ir_p.value_reference, ',', 2)::int
	where
	ir_p.name = 'property_payment_term_id'


	)


select
(
	case when (select count(deposit_id) from deposit_count) is not null
	then true
	else false
	end
	) as is_show_payment,
false as is_show_remark,
null as pos_id,
'-' as date_due,
sod.deposit_id as deposit_id,
sod.round_adjust as round_adjust,
null as non_deposit_inv_id,
false as is_show_receipt_no,
false as is_clear_deposit,
'' as receipt_no,
'' as origin_inv_no,
(select customer_addr from table_customer_addr where rp_id = cus_rpn.id) as customer_addr,
(
	case when sod.sale_payment_type = 'credit'
	then 'Credit Term'
	when sod.sale_payment_type = 'cash'
	then 'Cash / Credit'
	else 'Deposit'
	end
	)
as customer_payment_type,
true as is_show_delivery,
case when atx.amount is null
then 'N'
else 'V'
end as inv_line_tax,
CASE
WHEN sod.payment_term_id is null THEN coalesce(property.paymane_term_id,'-')
ELSE coalesce(apt.name,'-')
END as credit_term,
prp.id as inv_line_product_id,
prp.barcode as inv_line_product_barcode,
prt.name as inv_line_product_name,
(
	com_rpn.name  ||
	coalesce(
		case
			when pob.branch_id = '00000' 
			then ' สำนักงานใหญ่'
			else ' สาขา' || pob.name || ' สาขาที่ ' || coalesce(pob.branch_id,'')
		end, ''
		)
	) as company_name,
(
	coalesce(pob.street,'') || ' ' ||
	coalesce(pob.moo,'') || ' ' ||
	coalesce(pob.alley,'') || ' ' ||
	coalesce(pob.street2,'') || ' ' ||
	coalesce(
		case when com_prv.code = '10'
		then 'แขวง'
		else 'ตำบล'
		end || com_tmb.name, ''
		) || ' ' ||
	coalesce(
		case when com_prv.code = '10'
		then 'เขต'
		else 'อำเภอ'
		end || com_amp.name, ''
		) || ' ' ||
	coalesce(
		case when com_prv.name like '%กรุงเทพ%'
		then com_prv.name
		else ('จังหวัด' || com_prv.name)
		end, ''
		) || ' ' ||
	coalesce(com_zip.name, '')
	|| ' โทร.' || coalesce(pob.phone,'') ||
	coalesce('<br>เลขประจำตัวผู้เสียภาษีอากร: ' || com_rpn.vat, com_rpn.number, '')
	) as company_addr,
(
	case when sod.type_sale_ofm = true
	then
		TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'dd') || ' ' ||
		case when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '01'
			then 'มกราคม'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '02'
			then 'กุมภาพันธ์'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '03'
			then 'มีนาคม'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '04'
			then 'เมษายน'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '05'
			then 'พฤษภาคม'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '06'
			then 'มิถุนายน'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '07'
			then 'กรกฏาคม'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '08'
			then 'สิงหาคม'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '09'
			then 'กันยายน'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '10'
			then 'ตุลาคม'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '11'
			then 'พฤศจิกายน'
		when TO_CHAR(DATE(sod.date_deposit + interval '7 hours'), 'mm') = '12'
			then 'ธันวาคม'
		end || ' ' ||
		TO_CHAR(DATE(sod.date_deposit + interval '543 years'), 'yyyy')

	else
		TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'dd') || ' ' ||
		case when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '01'
			then 'มกราคม'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '02'
			then 'กุมภาพันธ์'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '03'
			then 'มีนาคม'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '04'
			then 'เมษายน'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '05'
			then 'พฤษภาคม'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '06'
			then 'มิถุนายน'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '07'
			then 'กรกฏาคม'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '08'
			then 'สิงหาคม'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '09'
			then 'กันยายน'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '10'
			then 'ตุลาคม'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '11'
			then 'พฤศจิกายน'
		when TO_CHAR(DATE(sod.date_order + interval '7 hours'), 'mm') = '12'
			then 'ธันวาคม'
		end || ' ' ||
		TO_CHAR(DATE(sod.date_order + interval '543 years'), 'yyyy')
	end
	) as date_invoice,
sod.deposit_name as doc_no,
case when sod.type_sale_ofm = true
then 'ใบเสร็จรับเงิน/ใบกำกับภาษี/ใบมัดจำ (Receipt/TaxInvoice/Deposit)'
else 'ใบเสร็จรับเงิน/ใบกำกับภาษี (Receipt/TaxInvoice)'
end as form_name,
'' as change_inv_detail,

cus_rpn.customer_code as customer_code,
(
	case when cus_rpn.customer_payment_type = 'credit'
	then 'Credit Term'
	when cus_rpn.customer_payment_type = 'cash'
	then 'Cash / Credit'
	else ''
	end
	) as customer_payment_type,
emp_rpn.name as employee,
(
	CASE WHEN cus_rpn.parent_id is null
	THEN coalesce(cus_rpn.name,'')
	ELSE coalesce(res_par.name,'')
	end ||' '||
	CASE WHEN cus_rpn.shop_id = '00000'
	THEN 'สำนักงานใหญ่'
	ELSE ''
	end
	|| ' ' ||
	CASE WHEN cus_rpn.branch_id is not null and cus_rpn.shop_id != '00000'
	THEN 'สาขาที่' || ' ' || cus_rpn.shop_id
	else ''
	end || ' ' ||
	coalesce(' เลขประจำตัวผู้เสียภาษีอากร: ' || cus_rpn.vat, cus_rpn.number, '')
)      as customer_name,
(
	coalesce(cus_delivery.name,'') || ' ' ||
	coalesce(cus_delivery.street,'') || ' ' ||
	coalesce(cus_delivery.moo,'') || ' ' ||
	coalesce(cus_delivery.alley,'') || ' ' ||
	coalesce(cus_delivery.street2,'') || ' ' ||
	coalesce(
		case when cus_delivery_prv.code = '10'
		then 'แขวง'
		else 'ตำบล'
		end || cus_delivery_tmb.name, ''
		) || ' ' ||
	coalesce(
		case when cus_delivery_prv.code = '10'
		then 'เขต'
		else 'อำเภอ'
		end || cus_delivery_amp.name, ''
		) || '  ' ||
	coalesce(
		case when cus_delivery_prv.name like '%กรุงเทพ%'
		then cus_delivery_prv.name
		else ('จังหวัด' || cus_delivery_prv.name)
		end, ''
		) || ' ' ||
	coalesce(cus_delivery_zip.name, '')
	) as delivery_addr,
con_rpn.name as contact_name,
coalesce(con_rpn.phone || ' ', '') ||
coalesce(con_rpn.mobile, '') as contact_phone,
prp.barcode as line_barcode,
sol.product_uom_qty as inv_line_quantity,
uom.name as line_uom,
sol.price_unit as inv_line_unit_price,
sol.price_unit as line_price_unit_discount,
sol.price_total as inv_line_amount,
sod.amount_total,
sod.amount_tax,
sod.doc_type,
sod.page,
round(
	case when atx.amount is null
	then 0
	else sol.price_unit * sol.product_uom_qty
	end
	) as inv_line_amount_subtotal,

(
	case when atx.amount is null
	then 'N'
	else 'V'
	end
	) as line_flag_tax,
sol.price_total as line_cal_total,
sol.promotion,
discount_non_taxed as discount_non_taxed,
discount_taxed as discount_taxed,
sol.prorate_amount + coalesce(promo_product.price_total, 0) as discount_rate,
(
	coalesce(sod.amount_delivery_fee_special, 0)
	+ coalesce(amount_delivery_fee_by_order, 0)
	) as delivery_fee
from (
	select so.*,dc.*,
	case when dc.print_time <= 1
	then 'ต้นฉบับ'
	else 'สำเนา'
	end as doc_type,
	1 as page
	from sale_order so
	left join deposit_count dc on dc.sale_id = so.id
	/* Parameter */
	where so.id in ($P!{IDS})
	and so.state not in ('draft')

	union all

	select so.*,dc.*,
	'สำเนา' as doc_type,
	2 as page
	from sale_order so
	left join deposit_count dc on dc.sale_id = so.id
	/* Parameter */
	where so.id in ($P!{IDS})
	and so.state not in ('draft')
	and dc.print_time <= 1
	) sod
left join deposit_count on deposit_count.sale_id = sod.id
inner join res_partner cus_rpn on sod.partner_id = cus_rpn.id
left join res_partner res_par on res_par.id = cus_rpn.parent_id

left JOIN province cus_prv ON cus_rpn.province_id = cus_prv.id
left JOIN amphur cus_amp ON cus_rpn.amphur_id = cus_amp.id
left JOIN tambon cus_tmb ON cus_rpn.tambon_id = cus_tmb.id
left JOIN zip cus_zip ON cus_rpn.zip_id = cus_zip.id
inner join res_partner cus_delivery on sod.partner_shipping_id = cus_delivery.id
left JOIN province cus_delivery_prv ON cus_delivery.province_id = cus_delivery_prv.id
left JOIN amphur cus_delivery_amp ON cus_delivery.amphur_id = cus_delivery_amp.id
left JOIN tambon cus_delivery_tmb ON cus_delivery.tambon_id = cus_delivery_tmb.id
left JOIN zip cus_delivery_zip ON cus_delivery.zip_id = cus_delivery_zip.id
inner join pos_branch pob on sod.branch_id = pob.id
inner join res_company rcn on pob.pos_company_id = rcn.id
inner join res_partner com_rpn on rcn.partner_id = com_rpn.id
left JOIN province com_prv ON pob.province_id = com_prv.id
left JOIN amphur com_amp ON pob.amphur_id = com_amp.id
left JOIN tambon com_tmb ON pob.tambon_id = com_tmb.id
left JOIN zip com_zip ON pob.zip_id = com_zip.id
inner join (
	select MIN(id) as id,
	order_id,
	product_id,
	product_uom,
	sum(product_uom_qty) as product_uom_qty,
	price_unit,
	sum(price_total) as price_total,
	MAX(delivery_fee_ofm) as delivery_fee_ofm,
	MAX(prorate_amount) as prorate_amount,
	promotion
	from sale_order_line
	/* Parameter */
	where order_id in ($P!{IDS})
	and promotion_name is null
	--and (is_type_delivery_special is false and is_type_delivery_by_order is false)
	group by order_id,
	product_id,
	product_uom,
	price_unit,
	promotion
	) sol on sod.id = sol.order_id
left join (
	select product_id,
	abs(price_total) as price_total
	from sale_order_line
	/* Parameter */
	where order_id in ($P!{IDS})
	and promotion is true
	and promotion_name is null
	) promo_product on sol.product_id = promo_product.product_id
inner JOIN product_product prp on sol.product_id = prp.id
inner JOIN product_template prt on prp.product_tmpl_id = prt.id
inner join product_uom uom on sol.product_uom = uom.id
inner join res_users emp_rus on sod.create_uid = emp_rus.id
inner join res_partner emp_rpn on emp_rus.partner_id = emp_rpn.id
left join account_payment_term apt on sod.payment_term_id = apt.id
left join ir_property_temp property on property.res_id = sod.partner_id


left join account_tax_sale_order_line_rel atp on atp.sale_order_line_id = sol.id
left join account_tax atx on atp.account_tax_id = atx.id
inner join res_partner con_rpn on sod.contact_id = con_rpn.id
inner join (
	select cal_dis_so.id as so_id,
	sum(
		case when cal_dis_atx.amount is null
		and cal_dis_sol.free_product_id is null
		then cal_dis_sol.prorate_amount
		when cal_dis_atx.amount is null
		and cal_dis_sol.free_product_id is not null
		then abs(cal_dis_sol.price_total)
		else 0
		end
		) as discount_non_taxed,
	sum(
		case when cal_dis_atx.amount is not null
		and cal_dis_sol.free_product_id is null
		then cal_dis_sol.prorate_amount
		when cal_dis_atx.amount is not null
		and cal_dis_sol.free_product_id is not null
		then abs(cal_dis_sol.price_total)
		else 0
		end
		) as discount_taxed
	from (
		select *
		from sale_order
		/* Parameter */
		where id in ($P!{IDS})
		) cal_dis_so
	inner join sale_order_line cal_dis_sol on cal_dis_so.id = cal_dis_sol.order_id
	left join account_tax_sale_order_line_rel cal_dis_atp on cal_dis_atp.sale_order_line_id = cal_dis_sol.id
	left join account_tax cal_dis_atx on cal_dis_atp.account_tax_id = cal_dis_atx.id

	group by cal_dis_so.id
	) cal_dis on sod.id = cal_dis.so_id
order by doc_type desc,sol.id]]>
	</queryString>
