	<subDataset name="journal_1">
		<parameter name="IDS" class="java.lang.Object"/>
		<queryString>
			<![CDATA[select
case when is_credit_card is true
     then card_type || ': ' || overlay(credit_card_no placing 'xxxxxx' from 7 for 6)
     else journal_name
end as journal_name,
journal_name p,
absl_id,
abs(sum(amount)) as amount,
journal_id,
card_type
from
(
select
absl.id as absl_id,
aj.name as journal_name,
aj.id as journal_id,
po.id,
po.name,
absl.amount,
absl.credit_card_no,
po.is_return_order,
aj.is_credit_card,
absl.credit_card_type card_type
from pos_order po
LEFT JOIN pos_session ps ON po.session_id = ps.id
LEFT JOIN pos_order_line pol ON po.id=pol.order_id
LEFT JOIN pos_config pf ON ps.config_id = pf.id
LEFT JOIN pos_branch pb ON pf.branch_id = pb.id
left join account_bank_statement_line absl on absl.pos_statement_id=po.id
left join account_journal aj on absl.journal_id=aj.id
where po.id in ($P!{IDS})
group by
aj.name,
aj.id,
po.id,
absl.amount,
po.is_return_order,
absl.id
)as pp
group by journal_name,journal_id,is_return_order,absl_id,credit_card_no,is_credit_card,card_type
order by absl_id]]>
		</queryString>
		<field name="journal_name" class="java.lang.String"/>
		<field name="p" class="java.lang.String"/>
		<field name="absl_id" class="java.lang.Integer"/>
		<field name="amount" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Amount]]></fieldDescription>
		</field>
		<field name="journal_id" class="java.lang.Integer"/>
		<field name="card_type" class="java.lang.String"/>
	</subDataset>
	<subDataset name="ds_discount">
		<parameter name="IDS" class="java.lang.Object"/>
		<queryString>
			<![CDATA[SELECT
	'ส่วนลดสินค้าที่คืน' AS promotion_name,
	sum(
		CASE WHEN free_product_id IS NOT NULL
		THEN price_unit * qty
		WHEN prorate_amount > 0
		THEN prorate_amount
		ELSE 0 END
	)
	AS order_line_subtotal
FROM
	pos_order_line
WHERE
	order_id IN ($P!{IDS})]]>
		</queryString>
		<field name="promotion_name" class="java.lang.String">
			<fieldDescription><![CDATA[Promotion]]></fieldDescription>
		</field>
		<field name="order_line_subtotal" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="ds_coupon">
		<parameter name="IDS" class="java.lang.Object"/>
		<parameter name="data_dir" class="java.lang.String"/>
		<queryString>
			<![CDATA[select ptp.name as coupon_name,
       (
        $P{data_dir} ||
        '/filestore/' ||
        current_database() ||
        '/' ||
        iam.store_fname
       ) as image_path,
       (
        case when pol.multi_coupon_id is null
             then prp.barcode
             else mcp.barcode
        end
       ) as barcode
from (
       select *
       from pos_order_line
       /* Parameter */
       where order_id in ($P!{IDS})
             and line_coupon = 'reward_coupon'
     ) pol
inner join product_product prp on pol.product_id = prp.id
inner join product_template ptp on prp.product_tmpl_id = ptp.id
left join ir_attachment iam on ptp.id = iam.res_id
                               and iam.res_model = 'product.template'
                               and iam.res_field = 'image_medium'
left join multi_coupon mcp on pol.multi_coupon_id = mcp.id
order by pol.id asc]]>
		</queryString>
		<field name="coupon_name" class="java.lang.String"/>
		<field name="image_path" class="java.lang.String"/>
		<field name="barcode" class="java.lang.String">
			<fieldDescription><![CDATA[Barcode]]></fieldDescription>
		</field>
	</subDataset>
	<parameter name="IDS" class="java.lang.Object">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="data_dir" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	company_logo,
	rpc.name AS company_name,
	coalesce(
			case when pos_b.branch_id = '00000'
				then ' สำนักงานใหญ่ '
				else ' สาขา '|| pos_b.branch_name ||' สาขาที่ '|| pos_b.branch_id
			end ,'' 
		)as branch_name,
	COALESCE(pos_b.phone,
	'') AS phone,
	rpc.vat AS tax_id,
	pos_ol.pos_no,
	pos_ol.price_unit,
	pos_ol.qty,
	pos_ol.price_unit* pos_ol.qty AS subtotal_line,
	('Return<br>' || prp.barcode) AS barcode,
	prt.name ||
	coalesce(' (' || prp.default_code || ')', '') AS name_template,
	case when prt.is_best_deal_promotion is true
		     then 'B'
		     else ''
		end
	|| pos_ol.iface_line_tax_included as iface_line_tax_included,
	pos_ol.inv_no,
	pos_ol.date_order,
	pos_ol.before_rounding,
	pos_ol.change_rounding,
	pos_ol.the_one_card_no,
	pos_ol.the_one_card_member_name,
	pos_ol.past_session,
	rtrs.name AS return_reason,
	rp.name AS sale_name,
	or_pos_o.inv_no AS receipt_no,
	or_pos_o.date_order::DATE AS purchase_date,
	or_pos_b.branch_id AS from_store,
	NOW() + INTERVAL '7 hour' AS print_date,
	disc.total_discount
FROM
	(
	SELECT
		pos_ol_sub.order_id,
		pos_ol_sub.product_id,
		pos_ol_sub.price_unit,
		SUM(pos_ol_sub.qty) AS qty,
		pos_ol_sub.iface_line_tax_included,
		pos_o.before_rounding,
		pos_o.change_rounding,
		pos_o.branch_id,
		pos_s.past_session,
		CASE
			WHEN pos_s.past_session = TRUE THEN pos_s.start_at + INTERVAL '7 hours'
			ELSE pos_o.date_order + INTERVAL '7 hours'
		END AS date_order,
		pos_o.inv_no,
		pos_o.return_order_id,
		pos_o.return_reason_id,
		pos_o.user_id,
		pos_o.the_one_card_no,
		pos_o.the_one_card_member_name,
		COALESCE(pos_c.pos_no,
		'XXXXXXXXXXXXX') AS pos_no
	FROM
		(
		SELECT
			order_id,
			product_id,
			price_unit,
			qty,
			CASE
				WHEN atx.amount IS NULL THEN 'N'
				ELSE 'V'
			END AS iface_line_tax_included
		FROM
			pos_order_line
		LEFT JOIN account_tax_pos_order_line_rel atp ON
			atp.pos_order_line_id = pos_order_line.id
		LEFT JOIN account_tax atx ON
			atp.account_tax_id = atx.id
		WHERE
			free_product_id ISNULL
			and line_coupon != 'reward_coupon'
			AND order_id IN ($P!{IDS})) pos_ol_sub
	INNER JOIN (
		SELECT
			id,
			before_rounding,
			change_rounding,
			inv_no,
			date_order,
			session_id,
			branch_id,
			return_order_id,
			return_reason_id,
			user_id,
			coalesce(the_one_card_no, '') as the_one_card_no,
			coalesce(member_name, '') as the_one_card_member_name
		FROM
			pos_order
		WHERE
			is_return_order IS TRUE
			AND id IN ($P!{IDS})) pos_o ON
		pos_ol_sub.order_id = pos_o.id
	INNER JOIN pos_session pos_s ON
		pos_o.session_id = pos_s.id
	INNER JOIN pos_config pos_c ON
		pos_s.config_id = pos_c.id
	WHERE
		pos_ol_sub.product_id <> pos_c.promotion_discount_product_id
	GROUP BY
		product_id,
		order_id,
		price_unit,
		iface_line_tax_included,
		pos_o.before_rounding,
		pos_o.change_rounding,
		pos_o.inv_no,
		pos_s.past_session,
		pos_s.start_at,
		pos_o.date_order,
		pos_o.branch_id,
		pos_o.return_order_id,
		pos_o.return_reason_id,
		pos_o.user_id,
		pos_o.the_one_card_no,
		pos_o.the_one_card_member_name,
		pos_no) pos_ol
INNER JOIN product_product prp ON
	pos_ol.product_id = prp.id
INNER JOIN product_template prt ON
	prp.product_tmpl_id = prt.id
INNER JOIN pos_branch pos_b ON
	pos_ol.branch_id = pos_b.id
INNER JOIN res_company res_c ON
	pos_b.pos_company_id = res_c.id
LEFT JOIN res_partner rpc ON
	res_c.partner_id = rpc.id
INNER JOIN return_reason rtrs ON
	pos_ol.return_reason_id = rtrs.id
INNER JOIN pos_order or_pos_o ON
	pos_ol.return_order_id = or_pos_o.id
INNER JOIN pos_branch or_pos_b ON
	or_pos_o.branch_id = or_pos_b.id
LEFT JOIN res_users AS ru ON
	ru.id = pos_ol.user_id
LEFT JOIN res_partner AS rp ON
	rp.id = ru.partner_id
CROSS JOIN (
	SELECT
		value || '/web/binary/company_logo?dbname=' || CURRENT_DATABASE() AS company_logo
	FROM
		ir_config_parameter
	WHERE
		KEY = 'report.image.url' ) AS comp
CROSS JOIN (
	SELECT
		SUM( CASE WHEN free_product_id IS NOT NULL
		          THEN price_unit * qty
		          WHEN prorate_amount > 0
		          THEN prorate_amount
		          ELSE 0
		     END ) AS total_discount
	FROM
		pos_order_line
	WHERE
		order_id IN ($P!{IDS})) AS disc
ORDER BY
	pos_ol.product_id,
	qty]]>
	</queryString>
