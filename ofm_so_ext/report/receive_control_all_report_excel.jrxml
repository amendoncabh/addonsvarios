	<style name="thai" lineSpacing="Single" fontName="TH SarabunPSK" fontSize="13"/>
	<parameter name="end_date" class="java.lang.String"/>
	<parameter name="start_date" class="java.lang.String"/>
	<parameter name="branch_id" class="java.lang.String"/>
	<parameter name="cashier_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[with 
sale_data as (
	select 
	so.branch_id,
	so.id as so_id,
	ai.id as ai_id,
	ad.id as ad_id,
	ad.payment_method,
	so.sale_payment_type,
	so.state as so_state,
	(case when so.sale_payment_type in ('cash','deposit') and ad.id notnull
			then case when ad.payment_method = 'Cash'
				then 'Cash'
				when ad.payment_method like 'Credit Card%'
				then 'Credit Card'
				else ad.payment_method 
				end
			when so.sale_payment_type = 'deposit' and ad.id is null
			then 'Cash'
			when so.sale_payment_type = 'credit' and ad.id is null
			then 'Credit Term'	
		else so.sale_payment_type
		end
	)as sale_payment_method,
	dsfc.total_zread as zread,
	so.type_sale_ofm as is_dropship,
	so.amount_total as so_total,
	ai.amount_total_signed as ai_total,
	ad.paid_total as ad_total,
	ai.type as ai_type,
	ad.type as ad_type,
	ai.state as ai_state,
	coalesce(ai.change_rounding,0.0) as ai_change_rounding,
	coalesce(ad.change_rounding,0.0) as ad_change_rounding
	from sale_order so 
	left join (
		select dsf.branch_id as branch_id,
		sum(dsfc.total) as total_zread 
		from daily_summary_franchise as dsf
		inner join daily_summary_franchise_cash as dsfc on dsf.id = dsfc.daily_summary_franchise_id
			  and dsf.state = 'active'
			  and dsfc.name = 'Cash (Sale Order)'
	          and dsf.branch_id = $P{branch_id}::integer
	          and (dsf.date + interval '7 hours')::date between $P{start_date}::date and $P{end_date}::date
	    group by dsf.branch_id
	) dsfc on so.branch_id = dsfc.branch_id
	left join account_invoice ai 
		on so.id = ai.so_id
		and ai.type in ('out_invoice')
		and ai.state in ('open','paid')
	left join (
			select	ad.id as id,
			ad.sale_id as sale_id,
			aj.name as payment_method,
			aj.is_credit_card as is_credit_card,
			coalesce(sum(adpl.paid_total),0.0) as paid_total,
			ad.state as state,
			ad.type,
			ad.change_rounding
			from account_deposit ad
			inner join account_deposit_payment_line adpl on ad.id = adpl.payment_id
			inner join account_payment_method_multi apmm on adpl.payment_method_id = apmm.id
			inner join account_journal aj on apmm.journal_id = aj.id
			group by ad.sale_id,
			ad.id,
			aj.name,
			aj.is_credit_card,
			ad.state
		) ad 
		on so.id = ad.sale_id
		and ad.state in ('open','paid')
	where 
		so.state in ('sale','done','cancel')
		and so.sale_payment_type is not null
		and so.branch_id = $P{branch_id}::integer
		and (so_date_order + interval '7 hours')::date between $P{start_date}::date and $P{end_date}::date
),
income as (
	select 
	(case when sd.is_dropship = true and sd.sale_payment_type in ('cash','deposit')
		then case when sd.ad_id is null 
					then coalesce(sd.ai_total,0)
				else coalesce(sd.ad_total,0)
				end
		when sd.is_dropship = true and sd.sale_payment_type = 'credit'
		then coalesce(sd.so_total,0)
		when sd.is_dropship = false and sd.sale_payment_type in ('cash','deposit')
		then case when sd.ad_id is null 
					then coalesce(sd.ai_total,0)
				else coalesce(sd.ad_total,0)
				end
		when sd.is_dropship = false and sd.sale_payment_type = 'credit'
		then coalesce(sd.so_total,0)
		end
	) as total,
	sd.sale_payment_method as sale_payment_method,
	coalesce(sd.zread,0.0) as zread,
	coalesce(case when sd.sale_payment_method like 'Credit Card%' or sd.sale_payment_method = 'Credit Term' then 0
		else case when sd.ad_id notnull and sd.ai_id is null then ad_change_rounding
				else sd.ai_change_rounding end
				end
	) as payment_rounding,	
	0.0 as return_rounding,
	'in'::text as type_invoice
	from sale_data sd
		where sd.ai_type = 'out_invoice'
			and sd.ad_type = 'out_deposit'
			and sd.is_dropship = false
			and sd.so_state in ('sale', 'done')
	union all
	select 
		coalesce(sd.so_total,0) as total,
		sd.sale_payment_method as sale_payment_method,
		coalesce(sd.zread,0.0) as zread,
		coalesce(case when sd.sale_payment_method like 'Credit Card%' or sd.sale_payment_method = 'Credit Term' then 0
		else case when sd.ad_id notnull and sd.ai_id is null then ad_change_rounding
				else sd.ai_change_rounding end
				end
	) as payment_rounding,	
	0.0 as return_rounding,
	'in'::text as type_invoice
	from sale_data sd
		where sd.sale_payment_type = 'credit'
		and sd.so_state in ('sale', 'done')
		and sd.is_dropship = false
	union all
	select 
	(case when sd.sale_payment_type in ('cash','deposit')
		then case when sd.ad_id is null 
					then coalesce(sd.ai_total,0)
				else coalesce(sd.ad_total,0)
				end
		when sd.sale_payment_type = 'credit'
		then coalesce(sd.so_total,0)
		end
	) as total,
	sd.sale_payment_method as sale_payment_method,
	coalesce(sd.zread,0.0) as zread,
	coalesce((case when sd.sale_payment_method like 'Credit Card%' or sd.sale_payment_method = 'Credit Term' then 0
		else case when sd.ad_id notnull and sd.ai_id is null then ad_change_rounding
				else sd.ai_change_rounding end
				end),0.0
	) as payment_rounding,	
	0.0 as return_rounding,
	'in'::text as type_invoice
	from sale_data sd
	where sd.is_dropship = true
	and sd.so_state in ('sale', 'done')
	union all 
	select 
	(case when sd.sale_payment_type in ('cash','deposit')
		then case when sd.ad_id is null 
					then coalesce(sd.ai_total,0)
				else coalesce(sd.ad_total,0)
				end
		when sd.sale_payment_type = 'credit'
		then coalesce(sd.so_total,0)
		end
	) as total,
	sd.sale_payment_method as sale_payment_method,
	coalesce(sd.zread,0.0) as zread,
	coalesce((case when sd.sale_payment_method like 'Credit Card%' or sd.sale_payment_method = 'Credit Term' then 0
		else case when sd.ad_id notnull and sd.ai_id is null then ad_change_rounding
				else sd.ai_change_rounding end
				end),0.0
	) as payment_rounding,	
	0.0 as return_rounding,
	'in'::text as type_invoice
	from sale_data sd
		where sd.so_state = 'cancel'
		and sd.ad_id notnull
),
outcome as (
	select 
	    coalesce(sum(paid_total), 0.00) as total,
	    ajn.name as sale_payment_method,
	    0.0 as zread,
	    0.0 as payment_rounding,
	    coalesce(ai.change_rounding,0.00) as return_rounding,
	    'out'::text as type_invoice
    from account_invoice ai 
	inner join (
    	select * from account_invoice
    ) invoice on invoice.id = ai.parent_invoice_id     
    inner join account_invoice_payment_line aipl on ai.id = aipl.invoice_id
    inner join account_payment_line apl on apl.payment_id = aipl.payment_id
    inner join account_payment_method_multi apmm on apmm.id = apl.payment_method_id
    inner join account_journal ajn on apmm.journal_id = ajn.id
    where 
    	ai.state = 'paid'
    	and ai.type = 'out_refund'
    	and ai.branch_id = $P{branch_id}::integer
		and (ai.date_invoice + interval '7 hours')::date between $P{start_date}::date and $P{end_date}::date
    group by aipl.invoice_id,
    ajn.name,
    ai.id,
    invoice.so_id
union
select 
	    coalesce(sum(paid_total)*-1, 0.00) as total,
	    ajn.name as sale_payment_method,
	    0.0 as zread,
	    0.0 as payment_rounding,
	    coalesce(ai.change_rounding,0.00) as return_rounding,
	    'out'::text as type_invoice
    from account_invoice ai      
    inner join account_deposit_account_invoice_rel adail on ai.id = adail.account_invoice_id
    inner join account_deposit_payment_line adpl on adpl.id = adail.account_deposit_id
    inner join account_payment_method_multi apmm on apmm.id = adpl.payment_method_id
    inner join account_journal ajn on apmm.journal_id = ajn.id
    where 
    	ai.state = 'paid'
    	and ai.type = 'out_refund'
    	and ai.branch_id = $P{branch_id}::integer
		and (ai.date_invoice + interval '7 hours')::date between $P{start_date}::date and $P{end_date}::date
    group by adpl.payment_id,
    ajn.name,
    ai.id 
),
company_info as (
    select
    rp_com.name  ||
    coalesce(
        CASE when pob.branch_id = '00000'
        then 'สำนักงานใหญ่'
        else ' สาขาที่ ' || pob.branch_id
        end
        , ''
        ) AS company_name,
    coalesce(pob.street,'') || ' ' ||
    coalesce(pob.moo,'') || ' ' ||
    coalesce(pob.alley,'') || ' ' ||
    coalesce(pob.street2,'') AS company_addr1,
    coalesce(case when pv_rp.code = '10'
        then 'แขวง'
        else 'ตำบล'
        end || tmb_rp.name,'') || ' ' ||
    coalesce(case when pv_rp.code = '10'
        then 'เขต'
        else 'อำเภอ'
        end || amp_rp.name,'') || ' ' ||
    coalesce(
        case when pv_rp.name like '%กรุงเทพ%'
        then pv_rp.name
        else ('จังหวัด' || pv_rp.name)
        end
        , ''
        ) || ' ' ||
    coalesce(zp_rp.name,'')
    || ' โทร.' || coalesce(pob.phone,'') AS company_addr2,
    'เลขประจำตัวผู้เสียภาษีอากร: ' || coalesce(rp_com.vat, rp_com.number) AS company_number
    from (
        SELECT *
        FROM pos_branch
        /* Parameter branch_id */
        WHERE id = $P{branch_id}::integer
        ) pob
    inner join res_company res_com ON pob.pos_company_id = res_com.id
    inner join res_partner rp_com ON res_com.partner_id = rp_com.id
    inner join res_partner rp_pob on pob.partner_id = rp_pob.id
    inner join province pv_rp ON rp_pob.province_id = pv_rp.id
    inner join amphur amp_rp ON rp_pob.amphur_id = amp_rp.id
    inner join tambon tmb_rp ON rp_pob.tambon_id = tmb_rp.id
    inner join zip zp_rp ON rp_pob.zip_id = zp_rp.id
),
prepare_sale_data as (
	select * 
	from income
	union all
	select * 
	from outcome
),
sale_data_final as (
	select 
		sale_payment_method as payment_type_name,
		coalesce(case when 
				type_invoice = 'in'
			then 
				sum(total)
			end,0.0 ) as amount_payment,
		coalesce(case when 
				type_invoice = 'in'
			then 
				sum(payment_rounding)
			end,0.0) as payment_rounding, 
		coalesce(case when 
				type_invoice = 'out'
			then 
				sum(total) 
			end,0.0) as return_system_amount,
		coalesce(case when 
				type_invoice = 'out'
			then 
				sum(return_rounding) 
			end,0.0) as return_rounding,
		coalesce(sum(total),0.0) as net_sales_amount,
		coalesce(sum(payment_rounding) - abs(sum(return_rounding)),0.0) as net_seles_rounding,
		coalesce(zread,0.0) as zread
	from 
		prepare_sale_data
	group by sale_payment_method,
		type_invoice,
		zread
),
backend as (
	select payment_type_name,
	coalesce(sum(amount_payment),0.0) as amount_payment,
	coalesce(sum(return_system_amount),0.0) as return_system_amount,
	coalesce(sum(net_sales_amount),0.0) as net_sales_amount,
	case when payment_type_name not in ('Cash')
		then coalesce(sum(net_sales_amount),0.0)
		else coalesce(sum(zread),0.0)
		end as zread_amount_payment,	
	sum(payment_rounding) as payment_rounding,
	sum(return_rounding) as return_rounding,
	sum(net_seles_rounding) as net_seles_rounding
from 
	sale_data_final
group by 
	payment_type_name
order by 
	payment_type_name 
),
pos as (
	SELECT case when redeem_type_id is not null
	then  rt.name
	else	payment_type
	end as payment_type_name,
	SUM(amount_payment) as amount_payment,
	SUM(return_system_amount) as return_system_amount,
	SUM(amount_payment) - abs(SUM(return_system_amount)) AS net_sales_amount,
	(
		case when is_credit_card is true or redeem_type_id is not null
		then SUM(amount_payment) - abs(SUM(return_system_amount))
		else sum(zread_amount_payment)
		end
		) as zread_amount_payment,
	sum(change_rounding) as payment_rounding,
	sum(return_rounding) as return_rounding,
	sum(change_rounding) + abs(sum(return_rounding)) as net_seles_rounding
	FROM (
		SELECT pss.payment_type AS payment_type,
		pss.is_credit_card,
		pss.redeem_type_id,
		sum(case when pss.is_return_order is false and pss.payment_type = 'Cash'
		    then change_rounding else 0 end
		   ) as change_rounding,
		sum(case when pss.is_return_order is true and pss.payment_type = 'Cash'
		    then change_rounding else 0 end
		   ) as return_rounding,
		sum(
			case when pss.is_return_order is false
			then pss.pos_order_values
			else 0
			end
			) AS amount_payment,
		sum(
			case when pss.is_return_order is true
			then pss.pos_order_values
			else 0
			end
			) AS return_system_amount,
		case when pss.is_credit_card is true
				then 0
			when pss.redeem_type_id is not null
				then 0
		else cash_abs.balance_end_real - cash_abs.balance_start
		end AS zread_amount_payment
		from (
			select sum(coalesce(abl.amount, 0)) AS pos_order_values,
			psi.user_id,
			psi.id as session_id,
			psi.cash_register_id,
			psi.company_name,
			psi.company_addr1,
			psi.company_addr2,
			psi.company_number,
			acj.name AS payment_type,
			acj.is_credit_card,
			acj.redeem_type_id,
			pod.is_return_order,
			pod.is_void_order,
			case when coalesce(abl.amount, 0) < 0 and pod.is_return_order is not true and pod.is_void_order is not true
			then 0
			else pod.change_rounding end
			from pos_order pod
			inner join (
				select *
				from pos_session psi
				inner join (
					select pcf1.id AS config_id,
					rpc.name  ||
					coalesce(
						CASE when pob.branch_id = '00000'
						then 'สำนักงานใหญ่'
						else ' สาขาที่ ' || pob.branch_id
						end
						, ''
						) AS company_name,
					coalesce(pob.street,'') || ' ' ||
					coalesce(pob.alley,'') || ' ' ||
					coalesce(pob.street2,'') || ' ' ||
					coalesce(pob.moo,'') || ' ' ||
					coalesce(pob.city,'') AS company_addr1,
					coalesce(
		case when pv.name like '%กรุงเทพ%'
		then pv.name
		else ('จังหวัด' || pv.name)
		end
		, ''
		) || ' ' ||
					coalesce(amp.name, '') || ' ' ||
					coalesce(tmb.name, '') || ' ' ||
					coalesce(zp.name,'')
					|| ' โทร.' || coalesce(pob.phone,'') AS company_addr2,
					'เลขประจำตัวผู้เสียภาษีอากร: ' || coalesce(rpc.vat, rpc.number) AS company_number
					from pos_config pcf1
					INNER JOIN (
						SELECT *
						FROM pos_branch
						/* Parameter branch_id */
						WHERE id = $P{branch_id}::integer
						) pob on pcf1.branch_id = pob.id
					left join res_country_state branch_province on pob.state_id = branch_province.id
					LEFT JOIN res_company rec ON pcf1.company_id = rec.id
					LEFT JOIN res_partner rpc ON rec.partner_id = rpc.id
					LEFT JOIN province pv ON pob.province_id = pv.id
					LEFT JOIN amphur amp ON pob.amphur_id = amp.id
					LEFT JOIN tambon tmb ON pob.tambon_id = tmb.id
					LEFT JOIN zip zp ON pob.zip_id = zp.id
					) pcf2 on psi.config_id = pcf2.config_id
				/* Parameter start_date, end_date, cashier_id */
				where DATE(stop_at + INTERVAL '7 hours') between $P{start_date}::date and $P{end_date}::date
				and (user_id = $P{cashier_id}::integer or coalesce($P{cashier_id}, '') = '')
				) psi on pod.session_id = psi.id
			left join account_bank_statement_line abl on pod.id = abl.pos_statement_id
			inner join account_journal acj on abl.journal_id = acj.id
			left join redeem_type rt on acj.redeem_type_id = rt.id
			group by pod.id,
			abl.amount,
			psi.user_id,
			psi.id,
			psi.cash_register_id,
			psi.company_name,
			psi.company_addr1,
			psi.company_addr2,
			psi.company_number,
			acj.is_credit_card,
			acj.redeem_type_id,
			acj.name,
			pod.is_return_order,
			pod.is_void_order,
			abl.journal_id
			) pss
		inner join account_bank_statement cash_abs on pss.cash_register_id = cash_abs.id
		inner join res_users rus on pss.user_id = rus.id
		inner join res_partner usr_rpn ON rus.partner_id = usr_rpn.id
		group by usr_rpn.id,
	         pss.session_id,
		pss.user_id,
		pss.payment_type,
		pss.is_credit_card,
		pss.redeem_type_id,
		pss.company_name,
		pss.company_addr1,
		pss.company_addr2,
		pss.company_number,
		cash_abs.balance_start,
		cash_abs.balance_end_real
		) result
		left join redeem_type rt on redeem_type_id = rt.id
	group by payment_type_name,
	is_credit_card,
	redeem_type_id,
	rt.name
),	
all_sale as (
	select
		case when payment_type_name like 'Credit Card%'
			then 'Credit Card'
			else payment_type_name
			end as payment_type_name,
		amount_payment,
		return_system_amount,
		net_sales_amount,
		zread_amount_payment,	
		payment_rounding,
		return_rounding,
		net_seles_rounding
	from backend bk
	union all
	select 
		* 
	from
		pos
)
select 
	payment_type_name,
	sum(amount_payment) as amount_payment,
	sum(return_system_amount) as return_system_amount,
	sum(net_sales_amount) as net_sales_amount,
	sum(zread_amount_payment) as zread_amount_payment,	
	company_logo,
	(select company_name from company_info) as company_name,
    (select company_addr1 from company_info) as company_addr1,
    (select company_addr2 from company_info) as company_addr2,
    (select company_number from company_info) as company_number,
	sum(payment_rounding) as payment_rounding,
	sum(return_rounding) as return_rounding,
	sum(net_seles_rounding) as net_seles_rounding,
	(now() + INTERVAL '7 hour')::DATE AS print_date,
	(now() + INTERVAL '7 hour')::TIME AS print_time
from all_sale
cross join (
			SELECT (value || '/web/binary/company_logo?dbname=' || current_database()) AS company_logo
			FROM ir_config_parameter
			WHERE key = 'report.image.url'
			) as company_logo
group by 
	payment_type_name,
	company_logo,
	company_name,
    company_addr1,
    company_addr2,
    company_number
order by payment_type_name
]]>
	</queryString>
	<field name="payment_type_name" class="java.lang.String"/>
	<field name="amount_payment" class="java.math.BigDecimal"/>
	<field name="return_system_amount" class="java.math.BigDecimal"/>
	<field name="net_sales_amount" class="java.math.BigDecimal"/>
	<field name="zread_amount_payment" class="java.math.BigDecimal"/>
	<field name="company_logo" class="java.lang.String"/>
	<field name="company_name" class="java.lang.String">
		<fieldDescription><![CDATA[Company Name]]></fieldDescription>
	</field>
	<field name="company_addr1" class="java.lang.String"/>
	<field name="company_addr2" class="java.lang.String"/>
	<field name="company_number" class="java.lang.String"/>
	<field name="print_date" class="java.sql.Date"/>
	<field name="print_time" class="java.sql.Time">
		<fieldDescription><![CDATA[Print time]]></fieldDescription>
	</field>
	<field name="payment_rounding" class="java.math.BigDecimal"/>
	<field name="return_rounding" class="java.math.BigDecimal"/>
	<field name="net_seles_rounding" class="java.math.BigDecimal"/>
	<variable name="diff_amount" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{zread_amount_payment} - $F{net_sales_amount}]]></variableExpression>
	</variable>
