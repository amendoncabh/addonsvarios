	<queryString>
		<![CDATA[select DATE(pco.date_planned + interval '7 hours') as date_planned,
       DATE(spk.date_done + interval '7 hours') as date_done,
       DATE(aiv.vendor_invoice_date + interval '7 hours') as date_invoice,
       spk.name as receipt_doc_no,
       aiv.reference as col_invoice_no,
       pco.name as po_doc_no,
       vendor_rpn.ref as vendor_code,
       vendor_rpn.name as vendor_name,
       requester_rpn.name as requester_name,
       pcg.cat_id as cat_id,
       pcg.sub_cat_id as sub_cat_id,
       CASE WHEN sdept.dept_parent_id ISNULL
            THEN sdept.ofm_dept_id
	    ELSE dept.ofm_dept_id
       END AS dept_id,
       CASE WHEN sdept.dept_parent_id ISNULL
            THEN ''
	    ELSE sdept.ofm_sub_dept_id
       END AS sub_dept_id,
       pdt.prod_status as product_status,
       prp.default_code as product_code,
       pdt.sku_ofm as sku,
       prp.barcode as barcode,
       pdt.name as product_name,
       pdu.name as product_uom_name,
       case when pco.state = 'draft'
            then 'PR'
            when pco.state = 'sent'
            then 'PR Sent'
            when pco.state = 'pending'
            then 'Pending'
            when pco.state = 'completed'
            then 'Completed'
            when pco.state = 'to approve'
            then 'To Approve'
            when pco.state = 'purchase'
            then 'Purchase Order'
            when pco.state = 'done'
            then 'Locked'
            when pco.state = 'cancel'
            then 'Cancelled'
       end as po_status,
       pco.date_order as po_create_date,
       pcl.product_qty as po_qty,
       pcl.qty_received as received_qty,
       pcl.price_unit as price_untaxed,
       pcl.price_subtotal as subtotal_price_untaxed,
       pcl.price_total as subtotal_price_taxed,
       pcl.qty_received * pcl.price_unit as subtotal_received_price_untaxed,
       (pcl.qty_received * pcl.price_unit) +
       (
        pcl.qty_received *
        (
         pcl.price_tax /
         case when pcl.product_qty = 0
              then 1
              else pcl.product_qty
         end
        )
       ) as subtotal_received_price_taxed,
       pco.amount_untaxed as total_amount_untaxed,
       pco.amount_tax as total_amount_tax,
       pco.amount_total as total_amount_taxed
from (
      select *
      from purchase_order
      /* Parameter */
      where (company_id = $P{company_id}::integer or COALESCE($P{company_id}, '') = '')
            and (branch_id = $P{branch_id}::integer or COALESCE($P{branch_id}, '') = '')
            and (name like '%' || $P{po_no} || '%' or COALESCE($P{po_no}, '') = '')
     ) pco
inner join purchase_order_line pcl on pco.id = pcl.order_id
inner join (
	    select *
            from stock_picking s_spk
            inner join (
                        select id as spt_id
			from stock_picking_type
			where code = 'incoming'
		       ) spt on s_spk.picking_type_id = spt.spt_id
            where state = 'done'
                  /* Parameter */
		  and (DATE(date_done + interval '7 hour') between $P{rd_start_date}::DATE and $P{rd_end_date}::DATE)
		  and (name like '%' || $P{rd_no} || '%' or COALESCE($P{rd_no}, '') = '')
           ) spk on pco.group_id = spk.group_id
left join (
	    select *
	    from account_invoice
	    where state not in ('draft', 'cancel')
	          and type = 'in_invoice'
	          /* Parameter */
	          and (reference like '%' || $P{col_inv_no} || '%' or COALESCE($P{col_inv_no}, '') = '')
	   ) aiv on spk.id = aiv.picking_id
left join res_partner vendor_rpn on pco.partner_id = vendor_rpn.id
inner join res_users requester_rus on pco.create_uid = requester_rus.id
inner join res_partner requester_rpn on requester_rus.partner_id = requester_rpn.id
inner join (
	    select *
	    from product_product
	    /* Parameter */
	    where (default_code like '%' || $P{product_code} || '%' or COALESCE($P{product_code}, '') = '')
	   ) prp on pcl.product_id = prp.id
inner join product_template pdt on prp.product_tmpl_id = pdt.id
LEFT JOIN ofm_product_dept sdept ON pdt.dept_ofm = sdept.id
LEFT JOIN ofm_product_dept dept ON sdept.dept_parent_id = dept.id
inner join product_uom pdu on pcl.product_uom = pdu.id
inner join stock_move stm on pcl.id = stm.purchase_line_id
inner join product_category pcg on pdt.categ_id = pcg.id
where ((DATE(aiv.vendor_invoice_date + interval '7 hour') between $P{inv_start_date}::DATE and $P{inv_end_date}::DATE)
	                or (COALESCE($P{inv_start_date}, '') = '' and COALESCE($P{inv_end_date}, '') = ''))
order by DATE(pco.date_planned + interval '7 hours'),
         spk.name,
         pcl.id]]>
	</queryString>
